<?xml version="1.0" encoding="UTF-8"?>
<a:screen xmlns:c="aurora.application.action" xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" tag="exp_report" trace="true">
    <a:init-procedure>
        <a:model-query model="expm.exp_report_document_page_type_find" rootPath="doc_page_type"/>
    </a:init-procedure>
    <a:view>
        <a:link/>
        <a:switch test="/model/doc_page_type/record/@document_page_type">
            <a:case value="TRAVEL">
                <a:screen-include screen="modules/gl/GL2020/exp_report_readonly_wfl_travel_finance_query_readonly.screen?exp_report_type_id=${/parameter/@exp_report_type_id}&amp;exp_report_header_id=${/parameter/@exp_report_header_id}"/>
            </a:case>
            <a:case value="STANDARD">
                <a:screen-include screen="modules/gl/GL2020/exp_report_readonly_wfl_standard_finance_query_readonly.screen?exp_report_type_id=${/parameter/@exp_report_type_id}&amp;exp_report_header_id=${/parameter/@exp_report_header_id}"/>
            </a:case>
        </a:switch>
    </a:view>
</a:screen>
