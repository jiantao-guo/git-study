                                        <a:column name="conditions_del" align="center" prompt="删除" renderer="conditionsDelFun" width="60"/>
                                    </a:columns>
                                    <a:editors>
                                        <a:comboBox id="comboBoxEditor"/>
                                    </a:editors>
                                </a:table>
