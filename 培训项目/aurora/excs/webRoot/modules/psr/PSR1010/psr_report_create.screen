                    unsynchronization_flag = record.get('unsynchronization_flag'),
                    synchronization_flag;
                var fullScreen = true;
                if (width && height) {
                    fullScreen = false;
                }
                if (unsynchronization_flag == 'Y') {
                    synchronization_flag = 'N';
