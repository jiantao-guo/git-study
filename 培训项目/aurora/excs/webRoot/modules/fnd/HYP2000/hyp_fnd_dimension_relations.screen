            <a:screenTopToolbar>
                <a:screenTitle/>
                <a:toolbarButton click="resetPurTypes" text="HAP_RESET" width="80"/>
                <a:gridButton bind="grid" type="add" width="80"/>
                <a:gridButton bind="grid" type="save" width="80"/>
