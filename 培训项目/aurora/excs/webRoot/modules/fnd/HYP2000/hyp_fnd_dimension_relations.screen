                        </a:mapping>
                    </a:field>
                    <a:field name="hec_dimension_name" prompt="FND_DIMENSIONS.DIMENSION_NAME"/>
                    <a:field name="enabled_flag" checkedValue="Y" defaultValue="Y" prompt="BGT_COMPANY_BUDGET_ITEMS.ENABLED_FLAG" uncheckedValue="N"/>
                </a:fields>
            </a:dataSet>
        </a:dataSets>
