                $('addBtn_total').enable();
                $('saveBtn_total').enable();
                $('runBtn_total').enable();
                $('deleteBtn_total').enable();
                if (records.length != 0) {
                    $('addBtn_total').disable();
                } else {
                    $('saveBtn_total').disable();
                    $('runBtn_total').disable();
                    $('deleteBtn_total').disable();
                }
            
            
