                </a:fields>
                <a:events>
                    <a:event name="add" handler="onReqLinesAdd"/>
                    <a:event name="update" handler="onLinesUpdateHandler"/>
                    <a:event name="submitsuccess" handler="onLinesSavedHandler"/>
                    <a:event name="beforecreate" handler="onadd"/>
                </a:events>
