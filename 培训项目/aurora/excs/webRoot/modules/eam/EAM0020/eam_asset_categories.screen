            function salvageValueRateValidator(record, name, value) {
                if (name == 'salvage_value_rate') {
                    var salvage_value_rate = record.get('salvage_value_rate');
                    if (salvage_value_rate < 0 || 100 < salvage_value_rate) {
                        return '${l:EAM_ASSET_FIN_CLASSES.SALVAGE_VALUE_RATE_VALIDATOR}';
                    }
                    return true;
                }
            }
            
            function onFinBeforeSubmitFun(ds) {
                // 验证
                var validate = $('eam0020_asset_fin_classes_ds').validate();
                if (!validate) {
                    return false;
                }
            
                var categories_result = $('eam0020_asset_categories_result_ds');
                var categories_record = categories_result.getCurrentRecord();
                var category_id = categories_record.get('category_id');
                var category_sec_id = categories_record.get('category_sec_id');
                var description_sec_id = categories_record.get('description_sec_id');
                if (category_id == ' ' || category_id == null) {
                    Aurora.showMessage('${l:PROMPT}', '${l:EAM_ASSET_CATEGORIES.DO_SAVE_CATEGORIES}');
                    return false;
                }
                var result_ds = $('eam0020_asset_fin_classes_ds');
                if (result_ds.validate()) {
                    var records = result_ds.getAll();
                    if (records.length < 1) {
                        Aurora.showMessage('${l:PROMPT}', '${l:EAM_ASSET_CATEGORIES.ADD_DATA}');
                    } else {
                        for (var i = 0;i < records.length;i++) {
                            var record = records[i];
                            record.set('asset_category_id', category_id);
                            record.set('asset_category_sec_id', category_sec_id);
                            record.set('description_sec_id', description_sec_id);
                        }
                    }
                }
            }
            
            function onFinBeUpdate(dataSet, record, name, value) {
                if (name == 'depreciate_flag') {
                    if (value == 'Y') {
                        record.getField("prorate_convention_desc").setRequired(true);
                        record.getField("depreciation_strategy_desc").setRequired(true);
                        record.getField("depreciation_months").setRequired(true);
                        record.getField("depreciation_method_desc").setRequired(true);
                    } else {
                        record.getField("prorate_convention_desc").setRequired(false);
                        record.getField("depreciation_strategy_desc").setRequired(false);
                        record.getField("depreciation_months").setRequired(false);
                        record.getField("depreciation_method_desc").setRequired(false);
                    }
                } else if (name == 'prorate_convention_code') {
                    if (value == 'CMP') {
                        record.set('depreciation_strategy', 'CMPC');
                        record.set('depreciation_strategy_desc', '启用当月开始折摊,处置当月不折摊');
                    } else if (value == 'NMP') {
                        record.set('depreciation_strategy', 'NMPC');
                        record.set('depreciation_strategy_desc', '启用下月开始折摊,处置当月仍计提折摊');
                    }
                }
            }
            
