            <!-- 由eam0020_asset_categories_query_ds获取出来的结果 -->
            <a:dataSet id="eam0020_asset_categories_result_ds" autoPageSize="true" autoQuery="true" fetchAll="false" model="eam.eam_asset_categories" queryDataSet="eam0020_asset_categories_query_ds" selectable="true" selectionModel="single">
                <a:fields>
                    <a:field name="asset_book_id" required="true"/>
