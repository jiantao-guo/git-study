            function accUsageCodeAssign(value, record, name) {
                var class_id = record.get('class_id');
                if (!record.isNew) {
                    return '<a href="javascript:accUsageCodeAssignInfo(' + class_id + ')">${l:EAM_ASSET_FIN_CLASSES.EAM_ACC_USAGE_CODE}</a>';
                }
            }
            
            function accUsageCodeAssignInfo(class_id) {
                new Aurora.Window({
                    id: 'acc_usage_code_assign_window',
                    url: $('acc_usage_code_assign_link').getUrl() + '?class_id=' + class_id,
                    title: '${l:EAM_ASSET_FIN_CLASSES.EAM_ACC_USAGE_CODE}',
                    height: 467,
                    width: 650
                });
            }
            
            function assetEntityClassesAssign(value, record, name) {
                var class_id = record.get('class_id');
                if (!record.isNew) {
                    return '<a href="javascript:assetEntityClassesAssignInfo(' + class_id + ')">${l:EAM_ASSET_FIN_CLASSES.EAM_FEATURE_PARAMETERS}</a>';
                }
            }
            
            function assetEntityClassesAssignInfo(class_id) {
                new Aurora.Window({
                    id: 'asset_entity_classes_assign_window',
                    url: $('asset_entity_classes_assign_link').getUrl() + '?class_id=' + class_id,
                    title: '${l:EAM_ASSET_FIN_CLASSES.EAM_FEATURE_PARAMETERS}',
                    height: 467,
                    width: 650
                });
            }
            
            function gridcellclick(grid, row, name, record) {
                var cate_record = $('eam0020_asset_categories_result_ds').getCurrentRecord();
                
                if(name == 'budget_item_desc'){
	                var MetaData = record.getMeta();
	                var MetaField = MetaData.getField('budget_item_desc');
	                MetaField.setLovPara('asset_book_id', cate_record.get('asset_book_id'));
	                //MetaField.setLovPara('budget_org_id', '${/model/exp_req_items_bgt_org_paths/record/@bgt_org_id}');
                }
                
                if (name == 'bgt_company_desc') {
                    if (cate_record.get('asset_book_id') != undefined && cate_record.get('asset_book_id') != '' && cate_record.get('asset_book_id') != null) {
                        record.getField("bgt_company_desc").setLovPara('asset_book_id', cate_record.get('asset_book_id'));
                    }
                }
                if (name == 'bgt_unit_desc') {
                    if (record.get('bgt_company_id') != undefined && record.get('bgt_company_id') != '' && record.get('bgt_company_id') != null) {
                        record.getField("bgt_unit_desc").setLovPara('company_id', record.get('bgt_company_id'));
                    }
                }
            }
            
            function entitygridcellclick(grid, row, name, record) {
                if (name == 'value_man_company_desc' || name == 'entity_man_company_desc') {
                    var categoriesRecord = $('eam0020_asset_categories_result_ds').getCurrentRecord();
                    if (categoriesRecord.get('asset_book_id') != undefined && categoriesRecord.get('asset_book_id') != '' && categoriesRecord.get('asset_book_id') != null) {
                        //record.getMeta().getField('value_man_unit_desc').setReadOnly(false);
                        record.getField("value_man_company_desc").setLovPara('asset_book_id', categoriesRecord.get('asset_book_id'));
                        record.getField("entity_man_company_desc").setLovPara('asset_book_id', categoriesRecord.get('asset_book_id'));
                    }
                }
                if (name == 'value_man_unit_desc') {
                    if (record.get('value_man_company_id') != undefined && record.get('value_man_company_id') != '' && record.get('value_man_company_id') != null) {
                        //record.getMeta().getField('value_man_unit_desc').setReadOnly(false);
                        record.getField("value_man_unit_desc").setLovPara('company_id', record.get('value_man_company_id'));
                    }
                }
                if (name == 'value_man_position_desc') {
                    if (record.get('value_man_unit_id') != undefined && record.get('value_man_unit_id') != '' && record.get('value_man_unit_id') != null) {
                        //record.getMeta().getField('value_man_position_desc').setReadOnly(false);
                        record.getField("value_man_position_desc").setLovPara('unit_id', record.get('value_man_unit_id'));
                    }
                }
                if (name == 'entity_man_unit_desc') {
                    if (record.get('entity_man_company_id') != undefined && record.get('entity_man_company_id') != '' && record.get('entity_man_company_id') != null) {
                        //record.getMeta().getField('entity_man_unit_desc').setReadOnly(false);
                        record.getField("entity_man_unit_desc").setLovPara('company_id', record.get('entity_man_company_id'));
                    }
                }
                if (name == 'entity_man_position_desc') {
                    if (record.get('entity_man_unit_id') != undefined && record.get('entity_man_unit_id') != '' && record.get('entity_man_unit_id') != null) {
                        //record.getMeta().getField('entity_man_position_desc').setReadOnly(false);
                        record.getField("entity_man_position_desc").setLovPara('unit_id', record.get('entity_man_unit_id'));
                    }
                }
                if (name == 'responsibility_employee_desc') {
                    if (record.get('entity_man_position_id') != undefined && record.get('entity_man_position_id') != '' && record.get('entity_man_position_id') != null) {
                        //record.getMeta().getField('entity_man_position_desc').setReadOnly(false);
                        record.getField("responsibility_employee_desc").setLovPara('position_id', record.get('entity_man_position_id'));
                    }
                }
            }
            
            function onFinAddFun(ds, record, index) {
                record.set('asset_category_id', $('eam0020_asset_categories_result_ds').getCurrentRecord().get('category_id'));
            }
            
            function onEntityAddFun(ds, record, index) {
                record.set('asset_category_id', $('eam0020_asset_categories_result_ds').getCurrentRecord().get('category_id'));
            }
            
            function onSelectCategoriesResultFun(ds, record) {
                //alert($('eam0020_asset_categories_result_ds').getCurrentRecord().get('category_id'));
                $('eam0020_asset_fin_classes_ds').setQueryParameter('asset_category_id', $('eam0020_asset_categories_result_ds').getCurrentRecord().get('category_id'));
                $('eam0020_asset_fin_classes_ds').query();
                $('eam0020_asset_entity_classes_ds').setQueryParameter('asset_category_id', $('eam0020_asset_categories_result_ds').getCurrentRecord().get('category_id'));
                $('eam0020_asset_entity_classes_ds').query();
            }
            
            function salvageValueRateValidator(record, name, value){
                if(name == 'salvage_value_rate'){
                    var salvage_value_rate = record.get('salvage_value_rate');
                    if(salvage_value_rate < 0 || 100 < salvage_value_rate){
                        return '${l:EAM_ASSET_FIN_CLASSES.SALVAGE_VALUE_RATE_VALIDATOR}';
                    }
                    return true;
                }
            }
            
            function onFinBeforeSubmitFun(ds) {
                // 验证
			    var validate = $('eam0020_asset_fin_classes_ds').validate();
			    if(!validate) {
			         return false;
			    }
			                
                var categories_result = $('eam0020_asset_categories_result_ds');
                var categories_record = categories_result.getCurrentRecord();
                var category_id = categories_record.get('category_id');
                if (category_id == ' ' || category_id == null) {
                    Aurora.showMessage('${l:PROMPT}', '${l:EAM_ASSET_CATEGORIES.DO_SAVE_CATEGORIES}');
                    return false;
                }
                var result_ds = $('eam0020_asset_fin_classes_ds');
                if (result_ds.validate()) {
                    var records = result_ds.getAll();
                    if (records.length < 1) {
                        Aurora.showMessage('${l:PROMPT}', '${l:EAM_ASSET_CATEGORIES.ADD_DATA}');
                    } else {
                        for (var i = 0;i < records.length;i++) {
                            var record = records[i];
                            record.set('asset_category_id', category_id);
                        }
                    }
                }
            }
            
            function onFinBeUpdate(dataSet, record, name, value) {
                if (name == 'depreciate_flag') {
                    if( value == 'Y' ) {
				    	record.getField("prorate_convention_desc").setRequired(true);
				    	record.getField("depreciation_strategy_desc").setRequired(true);
				    	record.getField("depreciation_months").setRequired(true);
				    	record.getField("depreciation_method_desc").setRequired(true);
                    }
                    else
                    {
				    	record.getField("prorate_convention_desc").setRequired(false);
				    	record.getField("depreciation_strategy_desc").setRequired(false);
				    	record.getField("depreciation_months").setRequired(false);
				    	record.getField("depreciation_method_desc").setRequired(false);
                    }
                } 
            }
            
            function onEntityBeforeSubmitFun(ds) {
                var categories_result = $('eam0020_asset_categories_result_ds');
                var categories_record = categories_result.getCurrentRecord();
                var category_id = categories_record.get('category_id');
                if (category_id == ' ' || category_id == null) {
                    Aurora.showMessage('${l:PROMPT}', '${l:EAM_ASSET_CATEGORIES.DO_SAVE_CATEGORIES}');
                    return false;
                }
                var result_ds = $('eam0020_asset_entity_classes_ds');
                if (result_ds.validate()) {
                    var records = result_ds.getAll();
                    if (records.length < 1) {
                        Aurora.showMessage('${l:PROMPT}', '${l:EAM_ASSET_CATEGORIES.ADD_DATA}');
                    } else {
                        for (var i = 0;i < records.length;i++) {
                            var record = records[i];
                            record.set('asset_category_id', category_id);
                        }
                    }
                }
            }
            
            function eam0020AstFinClassEdit(record,name){
                if(name == 'class_code'){
	                if (!record.isNew) {
	                    return '';
	                } else {
	                    return 'eam0020_ast_fin_classes_tf';
	                }
                }else if(name == 'depreciate_flag'){   
                    if (!record.isNew) {
	                    return '';
	                } else {
	                    return 'eam0020_ast_fin_classes_cb';
	                }
                }else if(name == 'off_sheet_flag'){   
                    if (!record.isNew) {
	                    return '';
	                } else {
	                    return 'eam0020_ast_fin_classes_cb';
	                }
                }else if(name == 'prorate_convention_desc'){
                    if (!record.isNew  || record.get('depreciate_flag') == 'N') {
	                    return '';
	                } else {
	                    return 'eam0020_ast_fin_classes_cbbox';
	                }
                }else if(name == 'depreciation_strategy_desc'){
                    if (!record.isNew  || record.get('depreciate_flag') == 'N') {
	                    return '';
	                } else {
	                    return 'eam0020_ast_fin_classes_cbbox2';
	                }
                }else if (name == 'depreciation_months'){
                    if (record.get('depreciate_flag') == 'N') {
                        return '';
                    } else {
	                    return 'eam0020_ast_fin_classes_nf2';
                    }
                }else if ( name == 'depreciation_method_desc') {
                    if (record.get('depreciate_flag') == 'N') {
                        return '';
                    } else {
	                    return 'eam0020_ast_fin_classes_cbbox2';
                    }
                }
            }
            function eam0020AstEntityClassEdit(record,name){
                if(name == 'class_code'){
	                if (!record.isNew) {
	                    return '';
	                } else {
	                    return 'eam0020_ast_entity_classes_tf';
	                }
                }else if(name == 'property_right_flag'){
                    if (!record.isNew) {
	                    return '';
	                } else {
	                    return 'eam0020_ast_entity_classes_cb';
	                }
                }else if(name == 'from_cip_flag'){
                    if (!record.isNew) {
	                    return '';
	                } else {
	                    return 'eam0020_ast_entity_classes_cb';
	                }
                }
            }
            
            function eam0020edit(record,name){
                if(name == 'asset_book_desc'){
                    if (!record.isNew) {
                    	return '';
	                } else {
	                    return 'eam0020_cbbox';
	                }
                }else if(name == 'asset_type_desc'){
               		if (!record.isNew) {
	                    return '';
	                } else {
	                    return 'eam0020_cbbox';
	                }     
                }else if(name == 'is_valuable_flag'){
                    if (!record.isNew) {
	                    return '';
	                } else {
	                    return 'eam0020_chkbox_v';
	                }
                }
            }
        ]]></script>
        <!-- datasets定义 -->
