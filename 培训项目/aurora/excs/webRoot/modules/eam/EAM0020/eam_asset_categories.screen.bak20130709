        <script><![CDATA[
            // 查询表单数据
            
            function eam0020_queryFun() {
                $('eam0020_asset_categories_result_ds').query();
            }
            
            // 重置表单
            
            function eam0020_resetFun() {
                $('eam0020_asset_categories_query_ds').reset();
            }
            
            function asset_categories_submit_success(){
                eam0020_queryFun();
            }
            // 设置资产大类编码的编辑器
            function eam0020_codeEditFun(record, name) {
                return !record.isNew ? '' : 'eam0020_txtfield_upper';
            }
            
            function eam0020AstEntityEditor(record, name) {
                if ((name == 'value_man_unit_desc' && record.get('value_man_company_id')) || (name == 'entity_man_unit_desc' && record.get('entity_man_company_id')) || (name == 'value_man_position_desc' && record.get('value_man_unit_id')) || (name == 'entity_man_position_desc' && record.get('entity_man_unit_id')) || (name == 'responsibility_employee_desc' && record.get('entity_man_position_id'))) {
                    return 'eam0020_ast_entity_classes_lv';
                } else {
                    return '';
                }
            
            }
            
            function onQueryDsUpdate(dataSet, record, name, value) {
                var lovField;
                if (name == 'value_man_company_id') {
                    lovField = record.getField("value_man_unit_desc");
                    lovField.setLovPara('company_id', record.get('value_man_company_id'));
                    record.set('value_man_unit_id', '');
                    record.set('value_man_unit_desc', '');
                    record.set('value_man_position_id', '');
                    record.set('value_man_position_desc', '');
                }
                if (name == 'value_man_unit_id') {
                    lovField = record.getField("value_man_position_desc");
                    lovField.setLovPara('unit_id', record.get('value_man_unit_id'));
                    record.set('value_man_position_id', '');
                    record.set('value_man_position_desc', '');
                }
                if (name == 'entity_man_company_id') {
                    lovField = record.getField("entity_man_unit_desc");
                    lovField.setLovPara('company_id', record.get('entity_man_company_id'));
                    record.set('entity_man_unit_id', '');
                    record.set('entity_man_unit_desc', '');
                    record.set('entity_man_position_id', '');
                    record.set('entity_man_position_desc', '');
                }
                if (name == 'entity_man_unit_id') {
                    lovField = record.getField("entity_man_position_desc");
                    lovField.setLovPara('unit_id', record.get('entity_man_unit_id'));
                    record.set('entity_man_position_id', '');
                    record.set('entity_man_position_desc', '');
                }
                if (name == 'entity_man_position_id') {
                    lovField = record.getField("responsibility_employee_desc");
                    lovField.setLovPara('position_id', record.get('entity_man_position_id'));
                }
            }
            
