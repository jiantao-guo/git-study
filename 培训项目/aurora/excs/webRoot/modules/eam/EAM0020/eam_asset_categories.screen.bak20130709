                        <a:mapping>
                            <a:map from="position_id" to="value_man_position_id"/>
                            <a:map from="position_code_name" to="value_man_position_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="entity_man_company_id"/>
                    <a:field name="entity_man_company_desc" lovGridHeight="300" lovHeight="450" lovService="eam.eam0030.eam0030_companies_vl_set_of_books_lov" lovWidth="550" title="BGT_BUDGET_ITEM_MAPPING.COMPANY_SEARCH">
                        <a:mapping>
                            <a:map from="company_id" to="entity_man_company_id"/>
                            <a:map from="company_short_name" to="entity_man_company_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="entity_man_unit_id"/>
                    <a:field name="entity_man_unit_desc" lovGridHeight="300" lovHeight="450" lovService="eam.eam0020.eam0020_exp_org_unit_lov" lovWidth="550" title="PUR_REQUISITION_ALLOCATE.UNIT_CODE_SEARCH">
                        <a:mapping>
                            <a:map from="unit_id" to="entity_man_unit_id"/>
                            <a:map from="unit_code_name" to="entity_man_unit_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="entity_man_position_id"/>
                    <a:field name="entity_man_position_desc" lovGridHeight="300" lovHeight="450" lovService="eam.eam0020.eam0020_exp_org_position_lov" lovWidth="550" title="BGT_BUDGET_ITEM_MAPPING.POSITION_CODE_SEARCH">
                        <a:mapping>
                            <a:map from="position_id" to="entity_man_position_id"/>
                            <a:map from="position_code_name" to="entity_man_position_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="responsibility_employee_id"/>
                    <a:field name="responsibility_employee_desc" lovGridHeight="300" lovHeight="450" lovService="eam.eam0020.eam0020_exp_employee_lov" lovWidth="550" title="EXP_EMPLOYEES.QUERY">
                        <a:mapping>
                            <a:map from="employee_id" to="responsibility_employee_id"/>
                            <a:map from="name" to="responsibility_employee_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="enabled_flag" checkedValue="Y" defaultValue="Y" unCheckedValue="N"/>
                    <a:field name="description" required="true"/>
                </a:fields>
                <a:events>
                    <a:event name="add" handler="onEntityAddFun"/>
                    <a:event name="update" handler="onQueryDsUpdate"/>
                    <a:event name="beforesubmit" handler="onEntityBeforeSubmitFun"/>
                </a:events>
            </a:dataSet>
        </a:dataSets>
        <!-- 资产类型搜索 -->
        <a:screenBody>
            <a:form column="3" labelWidth="100" title="EAM_ASSET_CATEGORIES.TITLE" width="1040">
                <a:comboBox name="asset_book_desc" bindTarget="eam0020_asset_categories_query_ds">
                    <a:events>
                        <a:event name="enterdown" handler="eam0020_queryFun"/>
                    </a:events>
                </a:comboBox>
                <a:textField name="category_code" bindTarget="eam0020_asset_categories_query_ds" typeCase="upper">
                    <a:events>
                        <a:event name="enterdown" handler="eam0020_queryFun"/>
                    </a:events>
                </a:textField>
                <a:textField name="category_name" bindTarget="eam0020_asset_categories_query_ds">
                    <a:events>
                        <a:event name="enterdown" handler="eam0020_queryFun"/>
                    </a:events>
                </a:textField>
            </a:form>
            <!-- 按钮 -->
            <a:hBox>
                <a:button click="eam0020_queryFun" text="HAP_QUERY"/>
                <a:button click="eam0020_resetFun" text="HAP_RESET"/>
            </a:hBox>
            <!-- 大类显示 -->
            <a:grid bindTarget="eam0020_asset_categories_result_ds" height="240" navBar="true" width="1040">
                <a:toolBar>
                    <a:button type="add"/>
                    <a:button type="save"/>
                    <a:button type="clear"/>
                </a:toolBar>
                <a:columns>
                    <a:column name="asset_book_desc" editorFunction="eam0020edit" sortable="true" width="120"/>
                    <a:column name="category_code" editorFunction="eam0020_codeEditFun" sortable="true" width="150"/>
                    <a:column name="description" editor="eam0020_txtfield" width="150"/>
                    <a:column name="asset_type_desc" editorFunction="eam0020edit" width="90"/>
                    <a:column name="is_valuable_flag" editorFunction="eam0020edit" width="100"/>
                    <a:column name="enabled_flag" editor="eam0020_chkbox_e" width="80"/>
                </a:columns>
                <a:editors>
                    <a:textField id="eam0020_txtfield"/>
                    <a:textField id="eam0020_txtfield_upper" typeCase="upper"/>
                    <a:comboBox id="eam0020_cbbox"/>
                    <a:checkBox id="eam0020_chkbox_v"/>
                    <a:checkBox id="eam0020_chkbox_e"/>
                </a:editors>
            </a:grid>
            <a:tabPanel id="eam0020_asset_classes_tab" height="280" width="1040">
                <a:tabs>
                    <a:tab height="100" prompt="EAM_ASSET_CATEGORIES.FIN_CLASS_INFO" width="80">
                        <a:grid id="eam0020_asset_fin_classes_grid" bindTarget="eam0020_asset_fin_classes_ds" height="250" width="1030">
                            <a:toolBar>
                                <a:button type="add"/>
                                <a:button type="save"/>
                                <a:button type="clear"/>
                            </a:toolBar>
                            <a:columns>
                                <a:column name="class_code" align="left" editorFunction="eam0020AstFinClassEdit" width="80"/>
                                <a:column name="description" editor="eam0020_ast_fin_classes_tf" width="100"/>
                                <a:column name="depreciate_flag" align="left" editorFunction="eam0020AstFinClassEdit" width="50"/>
                                <a:column name="prorate_convention_desc" align="left" editorFunction="eam0020AstFinClassEdit" width="80"/>
                                <a:column name="depreciation_months" align="right" editorFunction="eam0020AstFinClassEdit" width="80"/>
                                <a:column name="depreciation_method_desc" align="left" editorFunction="eam0020AstFinClassEdit" width="90"/>
                                <a:column name="depreciation_strategy_desc" align="left" editorFunction="eam0020AstFinClassEdit" width="150"/>
                                <a:column name="salvage_value_rate" align="right" editor="eam0020_ast_fin_classes_nf" width="60"/>
                                <a:column name="budget_item_desc" align="left" editor="eam0020_ast_fin_classes_lv" width="120"/>
                                <a:column name="bgt_company_desc" editor="eam0020_bgt_company_desc_lv" width="75"/>
                                <a:column name="bgt_unit_desc" editor="eam0020_bgt_unit_desc_lv" width="75"/>
                                <a:column name="off_sheet_flag" align="left" editorFunction="eam0020AstFinClassEdit" width="70"/>
                                <a:column name="enabled_flag" align="left" editor="eam0020_ast_fin_classes_cb" width="50"/>
                                <a:column name="class_id" align="left" renderer="accUsageCodeAssign" width="60"/>
                            </a:columns>
                            <a:events>
                                <a:event name="cellclick" handler="gridcellclick"/>
                            </a:events>
                            <a:editors>
                                <a:numberField id="eam0020_ast_fin_classes_nf" allowDecimals="true" allowNegative="false" decimalPrecision="3"/>
                                <a:numberField id="eam0020_ast_fin_classes_nf2" allowDecimals="false" allowNegative="false"/>
                                <a:textField id="eam0020_ast_fin_classes_tf" typeCase="upper"/>
                                <a:comboBox id="eam0020_ast_fin_classes_cbbox"/>
                                <a:comboBox id="eam0020_ast_fin_classes_cbbox2"/>
                                <a:lov id="eam0020_ast_fin_classes_lv"/>
                                <a:lov id="eam0020_bgt_company_desc_lv"/>
                                <a:lov id="eam0020_bgt_unit_desc_lv"/>
                                <a:checkBox id="eam0020_ast_fin_classes_cb"/>
                            </a:editors>
                        </a:grid>
                    </a:tab>
                    <a:tab height="100" prompt="EAM_ASSET_CATEGORIES.ENTITY_CLASS_INFO" width="80">
                        <a:grid id="eam0020_asset_entity_classes_grid" bindTarget="eam0020_asset_entity_classes_ds" height="250" width="1030">
                            <a:toolBar>
                                <a:button type="add"/>
                                <a:button type="save"/>
                                <a:button type="clear"/>
                            </a:toolBar>
                            <a:columns>
                                <a:column name="class_code" align="left" editorFunction="eam0020AstEntityClassEdit" width="80"/>
                                <a:column name="description" align="left" editor="eam0020_ast_entity_classes_tf" width="120"/>
                                <a:column name="property_right_flag" editorFunction="eam0020AstEntityClassEdit" width="60"/>
                                <a:column name="from_cip_flag" editorFunction="eam0020AstEntityClassEdit" width="60"/>
                                <a:column prompt="EAM_ASSET_ENTITY_CLASSES.VALUE_MAN">
                                    <a:column name="value_man_company_desc" editor="eam0020_ast_entity_classes_lv" width="75"/>
                                    <a:column name="value_man_unit_desc" editorFunction="eam0020AstEntityEditor" width="75"/>
                                    <a:column name="value_man_position_desc" editorFunction="eam0020AstEntityEditor" width="75"/>
                                </a:column>
                                <a:column prompt="EAM_ASSET_ENTITY_CLASSES.ENTITY_MAN">
                                    <a:column name="entity_man_company_desc" editor="eam0020_ast_entity_classes_lv" width="75"/>
                                    <a:column name="entity_man_unit_desc" editorFunction="eam0020AstEntityEditor" width="75"/>
                                    <a:column name="entity_man_position_desc" editorFunction="eam0020AstEntityEditor" width="75"/>
                                    <a:column name="responsibility_employee_desc" editorFunction="eam0020AstEntityEditor" width="75"/>
                                </a:column>
                                <a:column name="enabled_flag" editor="eam0020_ast_entity_classes_cb" width="40"/>
                                <a:column name="class_id" align="left" renderer="assetEntityClassesAssign" width="80"/>
                            </a:columns>
                            <a:events>
                                <a:event name="cellclick" handler="entitygridcellclick"/>
                            </a:events>
                            <a:editors>
                                <a:lov id="eam0020_ast_entity_classes_lv"/>
                                <a:textField id="eam0020_ast_entity_classes_tf" typeCase="upper"/>
                                <a:datePicker id="eam0020_ast_entity_classes_dp"/>
                                <a:numberField id="eam0020_ast_entity_classes_nf"/>
                                <a:checkBox id="eam0020_ast_entity_classes_cb"/>
                            </a:editors>
                        </a:grid>
                    </a:tab>
                </a:tabs>
            </a:tabPanel>
        </a:screenBody>
    </a:view>
</a:screen>
