            <a:dataSet id="eam0020_asset_fin_classes_ds" bindName="category" bindTarget="eam0020_asset_categories_result_ds" model="eam.eam_asset_fin_classes" selectable="true" submitUrl="${/request/@context_path}/autocrud/eam.eam0020.eam0020_asset_fin_classes_edit/batch_update">
                <a:fields>
                    <a:field name="asset_category_id"/>
                    <a:field name="class_id"/>
                    <a:field name="class_code" required="true"/>
                    <a:field name="description_id"/>
                    <a:field name="depreciate_flag" checkedValue="Y" defaultValue="N" unCheckedValue="N"/>
                    <a:field name="prorate_convention_code"/>
                    <a:field name="prorate_convention_desc" displayField="code_value_name" options="eam0020_prorate_convention_ds" returnField="prorate_convention_code" valueField="code_value"/>
                    <a:field name="depreciation_months"/>
                    <a:field name="depreciation_method"/>
                    <a:field name="depreciation_method_desc" displayField="code_value_name" options="eam0020_depreciation_method_ds" returnField="depreciation_method" valueField="code_value"/>
                    <a:field name="depreciation_strategy"/>
                    <a:field name="depreciation_strategy_desc" displayField="code_value_name" options="eam0020_depreciation_strategy_ds" returnField="depreciation_strategy" valueField="code_value"/>
                    <a:field name="salvage_value_rate" validator="salvageValueRateValidator"/>
