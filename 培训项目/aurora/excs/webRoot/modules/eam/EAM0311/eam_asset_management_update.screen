            function compareDate(start, end) {
                var startDate;
                var endDate;
                try {
                    startDate = new Date(start.replace(/-/g, "/"));
                } catch (e) {
                    startDate = start;
                }
                try {
                    endDate = new Date(end.replace(/-/g, "/"));
                } catch (e) {
                    endDate = end;
                }
                if (startDate > endDate) {
                    return false;
                }
                return true;
            }
            
