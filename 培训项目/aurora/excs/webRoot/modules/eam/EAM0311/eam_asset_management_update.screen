                    }
                    if (typeof(rent_date_to) != 'undefined' && !Ext.isEmpty(rent_date_to)) {
                        if (!compareDate(free_date_to, rent_date_to)) {
                            // return '免租日期到不能晚于出租日期到！';
                            return '免租日期从(到)必须在出租日期从(到)范围内！';
                        }
                    }
                    return true;
                }
            }
            
