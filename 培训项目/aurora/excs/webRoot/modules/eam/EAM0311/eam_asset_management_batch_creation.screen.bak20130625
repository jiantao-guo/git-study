                                <a:lov id="eam_batch_asset_lov"/>
                                <a:numberField id="eam_batch_numField" allowDecimals="true" allowFormat="true" allowNegative="false" decimalPrecision="2"/>
                                <a:comboBox id="eam_batch_asset_comboBox"/>
                            </a:editors>
                        </a:grid>
                    </a:tab>
                </a:tabs>
            </a:tabPanel>
        </a:screenBody>
    </a:view>
</a:screen>
