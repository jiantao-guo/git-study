            // 点击资产时将申请类型带入
            
            function eam0511_onCellClickFun(grid, row, name, record) {
                var ast_requisition_type_code = '${/model/eam0511_req_header/record/@ast_requisition_type_code}';
                if (name == 'tag_number') {
                    var lovField = record.getField("tag_number");
                    lovField.setLovPara('asset_book_id', '${/model/eam0511_req_header/record/@asset_book_id}');
                    //jhcw  category
                    // var category1, category2, category3, category4;
                    // if (ast_requisition_type_code == 'DB003' || ast_requisition_type_code == 'DB004') {
                    // category1 = "20";
                    // category2 = "20";
                    // category3 = "20";
                    // category4 = "20";
                    // } else {
                    // category1 = "10";
                    // category2 = "30";
                    // category3 = "0";
                    // category4 = "0";
                    // }
            
                    // lovField.setLovPara('category1', category1);
                    // lovField.setLovPara('category2', category2);
                    // lovField.setLovPara('category3', category3);
                    // lovField.setLovPara('category4', category4);
                    lovField.setLovPara('ast_requisition_type_code', ast_requisition_type_code);
                }
            
                if (name == 'transaction_type') {
                    var asset_book_id = '${/parameter/@asset_book_id}';
                    var base_trx_category = '${/parameter/@base_trx_category}';
                    $('eam0511_trans_type').setQueryParameter('asset_book_id', asset_book_id);
                    $('eam0511_trans_type').setQueryParameter('ast_requisition_type_id', '${/model/eam0511_req_header/record/@ast_requisition_type_id}');
                    $('eam0511_trans_type').setQueryParameter('base_trx_category', base_trx_category);
                    $('eam0511_trans_type').setQueryParameter('ast_requisition_type_code', ast_requisition_type_code);
                    $('eam0511_trans_type').query();
                }
            
                if (name == "to_company_desc") {
                    var to_company_desc = record.getField('to_company_desc');
                    // to_company_desc.setLovPara('company_id', '${/model/eam0511_req_header/record/@company_id}');
                    to_company_desc.setLovPara('asset_book_id', '${/parameter/@asset_book_id}');
                    to_company_desc.setLovPara('company_id', '${/session/@company_id}');
                }
                if (name == "to_employee_desc") {
                    var to_employee_desc = record.getField('to_employee_desc');
                    // to_company_desc.setLovPara('company_id', '${/model/eam0511_req_header/record/@company_id}');
                    var to_entity_unit_id = record.get('to_entity_unit_id');
                    to_employee_desc.setLovPara('unit_id', to_entity_unit_id);
                    to_employee_desc.setLovPara('company_id', record.get('to_company_id'));
                }
                if (name == "to_unit_desc") {
                    var to_unit_desc = record.getField('to_unit_desc');
                    to_unit_desc.setLovPara('base_trx_category', '${/parameter/@base_trx_category}');
                    to_unit_desc.setLovPara('responsibility_center_id', record.get('from_res_center_id'));
                    to_unit_desc.setLovPara('company_id', record.get('to_company_id'));
                }
                //点击状态时，如果接收部门为该资产的资产管理部门且接收人的角色为资产管理员，应资产状态栏位，可修改，可修改“使用中”或者“在库”
                if (name == "status_desc") {
                    //资产管理部门
                    var entity_unit_id = record.get('entity_unit_id');
                    //目标部门
                    var to_unit_id = record.get('to_unit_id');
                    //资产管理员角色标志
                    var asset_manage_flag = record.get('asset_manage_flag');
                    // if (!Ext.isEmpty(entity_unit_id) && entity_unit_id == to_unit_id && asset_manage_flag == "Y")
                    // {
                    // record.getField('status_desc').setReadOnly(false);
                    // }
                    // else
                    // {
                    // record.getField('status_desc').setReadOnly(true);
                    // }
                }
                //20151216
                if (name == "to_entity_company_desc") {
                    var to_entity_company_desc = record.getField('to_entity_company_desc');
                    to_entity_company_desc.setLovPara('asset_book_id', '${/parameter/@asset_book_id}');
                    to_entity_company_desc.setLovPara('company_id', '${/session/@company_id}');
                }
                //20151228
                if (name == "to_entity_unit_desc") {
                    var to_entity_unit_desc = record.getField('to_entity_unit_desc');
                    var to_entity_company_id = record.get('to_entity_company_id') ? record.get('to_entity_company_id') : -1;
                    to_entity_unit_desc.setLovPara('company_id', to_entity_company_id);
                }
            }
            
            function onLinesUpdateHandler(dataset, record, name, value) {
                var base_trx_category = '${/parameter/@base_trx_category}';
                switch (name) {
                case 'asset_code':
                    record.set('to_location', null);
                    break;
                case 'to_location_id':
                    record.getField("recipient").setLovPara("location_id", value);
                    record.set("recipient", null);
                    break;
                case 'from_location_id':
                    if (base_trx_category == 'DEP') {
                        record.getField('recipient').setLovPara("location_id", value);
                        record.set("recipient", null);
                    } else {
                        record.getField('recipient').setLovPara('location_id', null);
                        record.set('recipient', null);
                    }
                    break;
                case 'to_unit_desc':
                    record.set('to_employee_desc', '');
                    record.set('to_employee_id', '');
                    break;
                case 'tag_number':
                    record.set('to_unit_desc', '');
                    record.set('to_unit_id', '');
                    break;
                case 'to_employee_id':
                    //当接收人改变时，修改状态
                    //资产管理部门
                    /**
                     var entity_unit_id = record.get('entity_unit_id');
                     //目标部门
                     var to_unit_id = record.get('to_unit_id');
                     var asset_manage_flag = record.get('asset_manage_flag');
                     if (asset_manage_flag != 'Y' || entity_unit_id != to_unit_id)
                     {
                     record.set('status_desc',record.get('status_desc_bak'));
                     }
                     */
            
                    var entity_unit_id = record.get('entity_unit_id');
                    //目标部门
                    var to_unit_id = record.get('to_unit_id');
                    //资产管理员角色标志
                    var asset_manage_flag = record.get('asset_manage_flag');
                    if (!Ext.isEmpty(entity_unit_id) && entity_unit_id == to_unit_id && asset_manage_flag == "Y") {
                        //record.getField('status_desc').setReadOnly(false);
                    } else if (record.get('status') != "PARTIAL_SCRAPPED") {
                        record.set('status', 'USING');
                        record.set('status_desc', '使用中');
            
                        //record.getField('status_desc').setReadOnly(true);
                    }
            
                    break;
                case 'status':
                    if (value != 'PARTIAL_SCRAPPED') {
                        record.getField("status_desc").setReadOnly(false);
                    } else {
                        record.getField("status_desc").setReadOnly(true);
                    }
                    break;
                case 'to_company_desc':
                    record.set('to_unit_desc', '');
                    record.set('to_unit_id', '');
                    break;
                case 'to_entity_company_desc':
                    record.set('to_entity_unit_desc', '');
                    record.set('to_entity_unit_id', '');
                    break;
                case 'to_entity_unit_desc':
                    record.set('to_employee_desc', '');
                    record.set('to_employee_id', '');
                    break;
            
                }
            }
            
            
