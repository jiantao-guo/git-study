            function autoPercentValue() {
                //月折旧率计算公式为：月折旧额/（原始成本-残值）mount_dep_percent = month_dep_amount/(original_cost-salvage_value)
                // 残值计算公式为：当前成本*残值率，不可修改；salvage_value_rate
                // 剩余月数计算公式为：使用月数-已折旧月数，不可修改；
                var ast_requisition_type_code = '${/parameter/@ast_requisition_type_code}';
                var fin_records = $('eam_asset_finance_info_result_ds').getAt(0);
                var mount_dep_percent = "";
                var month_dep_amount = fin_records.get('month_dep_amount');
                var salvage_value_rate = fin_records.get('salvage_value_rate');
                var current_cost = fin_records.get('current_cost');
                var original_cost = fin_records.get('current_cost');
                var salvage_value = Math.ceil(salvage_value_rate * current_cost * 100) / 100;
                fin_records.set('salvage_value', salvage_value);
                if ($A.isEmpty(month_dep_amount)) {
                    fin_records.set('mount_dep_percent', '');
                }
                if (!$A.isEmpty(month_dep_amount) && !$A.isEmpty(salvage_value) && !$A.isEmpty(original_cost) && ast_requisition_type_code != 'QD005') {
                    mount_dep_percent = month_dep_amount / (original_cost - salvage_value);
                    fin_records.set('mount_dep_percent', mount_dep_percent);
                }
                //成本来源结转金额与当前成本一致
            
            }
            
