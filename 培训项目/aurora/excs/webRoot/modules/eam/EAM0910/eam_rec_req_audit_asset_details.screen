            function saveFun() {
                if (!$('eam0910_req_tra_acc_ds').validate()) {
                    return false;
                }
                if (!$('eam_asset_headers_readonly_result_ds').validate()) {
                    return false;
                }
                var accounting_flag = '${/model/inter_acc_flag/record/@accounting_flag}';
                var record = $('eam_asset_headers_readonly_result_ds').getCurrentRecord();
                record.set('to_location_id', record.get('location_id'));
                //$('eam_asset_headers_readonly_result_ds').submit();
                var fin_record = $('eam_asset_finance_info_result_ds').getCurrentRecord();
            
                var data = $('eam_asset_headers_readonly_result_ds').getJsonData();
            
                Aurora.request({
                    url: '${/request/@context_path}/modules/eam/EAM0910/eam_asset_management_save.svc',
                    para: data,
                    success: function(args) {
                        if (accounting_flag == 'Y') {
                            $('eam0910_req_tra_acc_ds').submit();
                            $('eam_asset_finance_info_result_ds').query();
                        }
                        Aurora.showWarningMessage('提示', '保存成功！', null, 240, 120);
                    }
                });
                // $('back_btn').disable();
            }
            
            function onSubSuccessHandler() {
                var requisition_line_id = '${/parameter/@requisition_line_id}';
                var asset_book_id = '${/parameter/@asset_book_id}';
                var asset_id = '${/parameter/@asset_id}';
            
                $('eam_asset_headers_readonly_result_ds').setQueryParameter('asset_book_id', asset_book_id);
                $('eam_asset_headers_readonly_result_ds').setQueryParameter('requisition_line_id', requisition_line_id);
                $('eam_asset_headers_readonly_result_ds').setQueryParameter('asset_id', asset_id);
                $('eam_asset_headers_readonly_result_ds').query();
                // alert('${l:BGT_BUDGET_STRUCTURE_DIMENSION.SAVE_SUC}');
            
                var record = $('eam_asset_finance_info_result_ds').getCurrentRecord();
                var currency_code = record.get("currency_code");
                var default_acc = $('tran_acc_default_val_ds').getCurrentRecord();
                var exchange_rate = default_acc.getField("exchange_rate");
            
                // if (currency_code == '${/model/sys_date/record/@cpy_currency_code}') {
                // exchange_rate.setReadOnly(true);
                // } else {
                // exchange_rate.setRequired(true);
                // exchange_rate.setReadOnly(false);
                // }
                default_acc.set('currency_code', currency_code);
                default_acc.set('requisition_line_id', requisition_line_id);
                var exchange_date = Aurora.formatDate(default_acc.get("journal_date"));
            
                Aurora.request({
                    url: $('get_exchange_rate').getUrl() + '?exchange_date=' + exchange_date + '&from_currency=' + currency_code,
                    success: function(args) {
                        var exchange_rate = args.result.record.exchange_rate;
                        var period_name = args.result.record.period_name;
            
                        default_acc.set('exchange_rate', exchange_rate);
                        default_acc.set('period_name', period_name);
                        $('eam0910_req_tra_acc_ds').setQueryParameter('requisition_line_id', requisition_line_id);
                        $('eam0910_req_tra_acc_ds').query();
                        var records = $('eam_asset_headers_readonly_result_ds').getAt(0);
                        var account_manual_flag = records.get("account_manual_flag");
                        if (account_manual_flag == 'N') {
                            //Ext.get('account_add').hide();
                            //Ext.get('account_save').hide();
                        }
                        // $('back_btn').enable();
                        setReadOnlyFun();
                    }
                });
            }
            
            function gridCellClick(grid, row, name, record) {
                if (name == 'company_desc') {
                    var asset_book_id = $('eam_asset_headers_readonly_result_ds').getCurrentRecord().get('asset_book_id');
                    if (asset_book_id != undefined && asset_book_id != '' && asset_book_id != null) {
                        record.getField("company_desc").setLovPara('asset_book_id', asset_book_id);
                    }
                }
                if (name == 'responsibility_center_desc') {
                    if (record.get('company_id') != undefined && record.get('company_id') != '' && record.get('company_id') != null) {
                        record.getMeta().getField('responsibility_center_desc').setReadOnly(false);
                        record.getField("responsibility_center_desc").setLovPara('company_id', record.get('company_id'));
                    }
                }
            }
            
            function onRendererFun(value, record, name) {
                var record_dv = $('tran_acc_default_val_ds').getAt(0);
                var entered_amount_dr = record.get('entered_amount_dr');
                var entered_amount_cr = record.get('entered_amount_cr');
                if (entered_amount_dr == '0') {
                    if (name == 'functional_amount_dr') {
                        record.set('functional_amount_dr', '0');
                        return Aurora.formatMoney(record.get('entered_amount_dr'));
                    }
                }
                if (record.get('entered_amount_dr')) {
                    if (name == 'functional_amount_dr') {
                        record.set('functional_amount_dr', record.get('entered_amount_dr'));
                        return Aurora.formatMoney(record.get('entered_amount_dr'));
                    }
                }
                if (entered_amount_cr == '0') {
                    if (name == 'functional_amount_cr') {
                        record.set('functional_amount_cr', '0');
                        return Aurora.formatMoney(record.get('entered_amount_cr'));
                    }
                }
                if (record.get('entered_amount_cr')) {
                    if (name == 'functional_amount_cr') {
                        record.set('functional_amount_cr', record.get('entered_amount_cr'));
                        return Aurora.formatMoney(record.get('entered_amount_cr'));
                    }
                }
            }
            
            
            function onCostSourceAdd(dataSet, record, index) {
