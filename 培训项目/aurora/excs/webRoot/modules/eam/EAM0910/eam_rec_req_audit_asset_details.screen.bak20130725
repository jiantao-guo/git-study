        	    var currency_code = record.get("currency_code");
                var default_acc = $('tran_acc_default_val_ds').getCurrentRecord();
        	    var exchange_rate = default_acc.getField("exchange_rate");
                
    		    if(currency_code == '${/model/sys_date/record/@cpy_currency_code}') {
    		        exchange_rate.setReadOnly(true); 
    		    } else {
    		        exchange_rate.setRequired(true);
