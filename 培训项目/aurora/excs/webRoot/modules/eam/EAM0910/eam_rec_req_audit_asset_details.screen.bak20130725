        	        tabs.setDisabled(1);
        	    }
        	    
        	    /* var inter_flag = '${/model/inter_acc_flag/record/@interface_flag}';
                if(inter_flag == 'Y') {
                    var record1 = $('eam_asset_finance_info_result_ds').getCurrentRecord();
                    record1.getMeta().getField('asset_card_num').setRequired(true);
                } */
                var form_record = $('eam_asset_headers_readonly_result_ds').getCurrentRecord();
                
                var price = '${/model/req_line/record/@price}';
                var req_desc = '${/model/req_line/record/@description}';
                var transaction_type_id = '${/model/req_line/record/@transaction_type_id}';
                var transaction_type = '${/model/req_line/record/@trans_type_desc}';
        	     
        	    form_record.set('price', price);
        	    form_record.set('req_desc', req_desc);
        	    form_record.set('transaction_type_id', transaction_type_id);
        	    form_record.set('transaction_type', transaction_type);
        	     
                var records = $('cost_source_ds').getAll();
              	var value = form_record.get("asset_source_type");
              	
              	var fi_record = $("eam_asset_finance_info_result_ds").getCurrentRecord();
              	
              	var requisition_line_id = '${/parameter/@requisition_line_id}';
              	// 如果财务信息存在
              	if('${/model/asset_header/record/@exists_fi}' == 1) {
              	    $("eam_asset_finance_info_result_ds").query();
              	    
              	    $("tran_acc_default_val_ds").setQueryParameter("requisition_line_id", requisition_line_id);
              	    $("tran_acc_default_val_ds").setQueryParameter("asset_id", '${/parameter/@asset_id}');
              	    $("tran_acc_default_val_ds").query();
              	} else {
              	    //  如果不存在财务信息
              	    Aurora.request({
                    url: $('get_exchange_rate').getUrl() + '?exchange_date=' + '${/model/req_line/record/@default_journal_date}' + '&from_currency=' + '${/model/asset_header/record/@currency_code}',
                    success: function(args) {
                        var exchange_rate = args.result.record.exchange_rate;
                        var period_name = args.result.record.period_name;
                        
                        var default_acc = $("tran_acc_default_val_ds").getCurrentRecord();
                        default_acc.set('exchange_rate', exchange_rate);
                        default_acc.set('period_name', period_name);
