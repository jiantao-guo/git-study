                        </a:grid>
                    </a:tab>
                </a:tabs>
            </a:tabPanel>
            <!-- 单据关联 -->
            <div id="grid_release_id">
                <a:grid id="release_id" bindTarget="eam0910_document_release_ds" height="200" navBar="true" width="1024">
                    <a:toolBar>
                        <a:label/>
                    </a:toolBar>
                    <a:columns>
                        <a:column name="source_doc_line_desc" align="center" width="40"/>
                        <a:column name="source_doc_cate_desc" align="center"/>
