                    <a:column name="source_asset_name" align="center"/>
                    <a:column name="tag_number" align="center" prompt="暂估资产卡片号" width="140"/>
                    <a:column name="dest_asset_name" align="center"/>
                    <!-- <a:column name="req_line_num" align="center"/> -->
