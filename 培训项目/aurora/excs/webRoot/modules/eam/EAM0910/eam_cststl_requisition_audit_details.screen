                    <a:column name="final_cost" align="right" renderer="Aurora.formatMoney" width="90"/>
                    <a:column name="setlement_date" align="center" renderer="Aurora.formatDate"/>
                    <a:column name="amortize_adj" align="center" width="80"/>
                    <a:column name="description" align="center" width="135"/>
                </a:columns>
                <a:editors>
                    <a:textField id="txtField"/>
                    <a:numberField id="numField" allowDecimals="true" allowFormat="true" decimalPrecision="2"/>
                    <a:datePicker id="datePicker"/>
                </a:editors>
            </a:grid>
            <a:tabPanel id="eam0910_tran_accounts_tab" height="350" style="display: none;" width="1024">
                <a:tabs>
                    <a:tab id="acc_transfer" prompt="EAM_REQUISITION_ACCOUNTING_VOCHERS">
                        <a:form column="1" height="20" labelWidth="100" width="890">
                            <a:box column="4">
                                <a:datePicker name="journal_date" bindTarget="tran_acc_default_val_ds" renderer="Aurora.formatDate"/>
                                <!-- <a:textField name="currency_code" bindTarget="tran_acc_default_val_ds"/> -->
                                <!-- <a:textField name="exchange_rate" bindTarget="tran_acc_default_val_ds"/> -->
                                <a:button click="create_account" text="PROMPT.CREATE_JE"/>
                            </a:box>
                        </a:form>
                        <a:grid bindTarget="eam0910_req_tra_acc_ds" height="220" navBar="true" width="1015">
                            <a:toolBar>
                                <a:button id="account_add" type="add"/>
                                <a:button id="account_save" type="save"/>
                                <a:button type="delete"/>
                            </a:toolBar>
                            <a:columns>
                                <a:column name="description" align="center" editorFunction="eam0910accountflag" width="158"/>
                                <a:column name="journal_date" align="center" renderer="Aurora.formatDate" width="80"/>
                                <a:column name="period_name" align="center" width="80"/>
                                <a:column name="company_desc" align="center" editor="eam0910_company_lv" editorFunction="eam0910accountflag" width="120"/>
                                <a:column name="responsibility_center_desc" align="center" editor="eam0910_resp_lv" editorFunction="eam0910accountflag" width="150"/>
                                <a:column name="account_code" align="center" width="100"/>
                                <a:column name="account_desc" align="center" editorFunction="eam0910accountflag" width="100"/>
                                <a:column name="entered_amount_dr" align="right" editorFunction="eam0910accountflag" renderer="Aurora.formatMoney" width="100"/>
                                <a:column name="entered_amount_cr" align="right" editorFunction="eam0910accountflag" renderer="Aurora.formatMoney" width="100"/>
                                <!--  <a:column prompt="EAM_TRANSACTION_ACCOUNTS.ENTERED_AMOUNT">
                                    <a:column name="entered_amount_dr" editor="eam0910_amount_nf" editorFunction="eam0910accountflag" renderer="Aurora.formatMoney" width="75"/>
                                    <a:column name="entered_amount_cr" editor="eam0910_amount_nf" editorFunction="eam0910accountflag" renderer="Aurora.formatMoney" width="75"/>
                                </a:column>
                                <a:column prompt="EAM_TRANSACTION_ACCOUNTS.FUNCTIONAL_AMOUNT">
                                    <a:column name="functional_amount_dr" renderer="onRendererFun" width="75"/>
                                    <a:column name="functional_amount_cr" renderer="onRendererFun" width="75"/>
                                </a:column> -->
                            </a:columns>
                            <a:editors>
                                <a:textField id="eam0910_desc_tf"/>
                                <a:lov id="eam0910_account_lv"/>
                                <a:lov id="eam0910_company_lv"/>
                                <a:lov id="eam0910_resp_lv"/>
                                <a:numberField id="eam0910_amount_nf" allowDecimals="true" allowFormat="true" allowNegative="true" decimalPrecision="3"/>
                            </a:editors>
                            <a:events>
                                <a:event name="cellclick" handler="gridCellClick"/>
                            </a:events>
                        </a:grid>
                    </a:tab>
                    <a:tab id="eam0910_cost_vadj_req_tab" prompt="EAM_ASSET_REQUISITION.REF_DOCUMENT" width="80">
                        <!-- 单据关联 -->
                        <a:grid bindTarget="eam0910_document_release_ds" height="260" navBar="true" width="1024">
                            <a:columns>
                                <a:column name="source_doc_line_desc" align="center" width="40"/>
                                <a:column name="source_doc_cate_desc" align="center"/>
                                <a:column name="source_doc_desc" align="center" width="160"/>
                                <a:column name="req_project" align="center"/>
                                <a:column name="req_quantity" align="right"/>
                                <a:column name="req_amount" align="right" renderer="Aurora.formatMoney"/>
                                <!-- <a:column name="release_quantity" align="center" editor="rec_req_quantity"/> -->
                                <a:column name="release_amount" align="right" renderer="Aurora.formatMoney"/>
                                <a:column name="description" align="center" width="150"/>
