<?xml version="1.0" encoding="UTF-8"?>
<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:view>
        <script><![CDATA[
           function backtomain(){
               $('gld_work_order_sql_validate_window').close();
           }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="gld_ref_obj_tmp_ds" autoCount="true" autoQuery="true" fetchAll="false" model="gld.GLD1030.gld_work_order_ref_sql_tmp"/>
        </a:dataSets>
        <a:grid bindTarget="gld_ref_obj_tmp_ds" height="400" navBar="true" style="margin-top:10px;margin-left:10px" width="400">
            <a:columns>
                <a:column name="ref_field_code" prompt="EXP_OBJ_VALUE_LIST.OBJ_CODE" width="200"/>
                <a:column name="description" prompt="EXP_OBJ_VALUE_LIST.DESC" width="200"/>
            </a:columns>
        </a:grid>
        <a:hBox>
            <a:button click="backtomain" style="margin-top:10px;margin-left:10px" text="PROMPT.RETURN"/>
        </a:hBox>
    </a:view>
</a:screen>
