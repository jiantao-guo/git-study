<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure>
        <a:model-query model="mobile.systems.hr.ssv.hr_ssv_leave_cancel_for_query" rootPath="cancel_detail"/>
    </a:init-procedure>
    <a:view template="mobile">
        <div style="margin:5px;">
            <script><![CDATA[
        
        ]]></script>
            <a:formView dataModel="/model/cancel_detail" promptAlign="left" title="休假撤销申请" viewType="simple">
                <a:formSecion>
                    <a:field name="unit_name" prompt="部门" promptWidth="40" width="60"/>
                </a:formSecion>
                <a:formSecion>
                    <a:field name="employee_code" prompt="工号 " promptWidth="40" width="60"/>
                </a:formSecion>
                <a:formSecion>
                    <a:field name="name" prompt="姓名" promptWidth="40" width="60"/>
                </a:formSecion>
                <a:formSecion>
                    <a:field name="item_name" prompt="休假项目" promptWidth="40" width="60"/>
                </a:formSecion>
                <a:formSecion>
                    <a:field name="date_from" prompt="开始时间" promptWidth="40" width="60"/>
                </a:formSecion>
                <a:formSecion>
                    <a:field name="date_to" prompt="结束时间" promptWidth="40" width="60"/>
                </a:formSecion>
                <a:formSecion>
                    <a:field name="total_days" prompt="时间" promptWidth="40" width="60"/>
                </a:formSecion>
                <a:formSecion>
                    <a:field name="cancel_desc" prompt="说明" promptWidth="40" width="60"/>
                </a:formSecion>
            </a:formView>
        </div>
    </a:view>
</a:screen>
