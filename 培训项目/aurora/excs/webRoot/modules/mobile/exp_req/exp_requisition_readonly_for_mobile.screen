<?xml version="1.0" encoding="UTF-8"?>
<a:screen xmlns:a="http://www.aurora-framework.org/application" xmlns:p="uncertain.proc" trace="true">
    <a:init-procedure>
        <a:model-query model="mobile.exp_requisition.exp_requisition_header_for_mobile" rootPath="exp_req_header"/>
        <a:model-query model="mobile.exp_requisition.exp_requisition_header_dimension_for_mobile" rootPath="exp_req_header_dim"/>
        <a:model-query model="mobile.exp_requisition.exp_requisition_header_obj_for_mobile" rootPath="exp_req_header_obj"/>
        <a:model-query model="mobile.exp_requisition.exp_requisition_line_for_mobile" rootPath="exp_req_line"/>
        <a:model-query model="mobile.exp_requisition.exp_requisition_line_dim_for_mobile" rootPath="exp_req_line_dim"/>
        <a:model-query model="mobile.exp_requisition.exp_requisition_line_obj_for_mobile" rootPath="exp_req_line_obj"/>
        <!-- 
        <a:model-query model="mobile.exp_report.exp_report_line_dim_for_mobile" rootPath="exp_report_line_dim"/>
        <a:model-query model="mobile.exp_report.exp_report_line_obj_for_mobile" rootPath="exp_report_line_obj"/>  -->
    </a:init-procedure>
    <a:view template="mobile">
        <div id="line_frame">
            <div class="line_title">
                <span><![CDATA[申请头信息]]></span>
            </div>
            <div class="tab_content">
                <table class="line_content">
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[单据编号：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@exp_requisition_number}]]></span>
                        </td>
                    </tr>
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[员工：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@employee_name}]]></span>
                        </td>
                    </tr>
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[公司：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@company_name}]]></span>
                        </td>
                    </tr>
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[岗位：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@position_id_display}]]></span>
                        </td>
                    </tr>
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[部门：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@unit_name}]]></span>
                        </td>
                    </tr>
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[申请日期：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@requisition_date}]]></span>
                        </td>
                    </tr>
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[币种：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@currency_name}]]></span>
                        </td>
                    </tr>
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[收款方：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@payee_code} (${/model/exp_req_header/record/@payee_category_name} )]]></span>
                        </td>
                    </tr>
                    <!--<tr>
                        <td class="line_content_title">
                            <span><![CDATA[收款方：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@payee_name}]]></span>
                        </td>
                    </tr>-->
                    <tr>
                        <td class="line_content_title">
                            <span><![CDATA[制单人：]]></span>
                        </td>
                        <td class="line_content_value">
                            <span><![CDATA[${/model/exp_req_header/record/@created_by}]]></span>
                        </td>
                    </tr>
                    <!-- 头维度信息 -->
                    <a:repeater datamodel="/model/exp_req_header_dim">
                        <tr>
                            <td class="line_content_title">
                                <span><![CDATA[${@dimension_name}：]]></span>
                            </td>
                            <td class="line_content_value">
                                <span><![CDATA[${@dimension_value_name}]]></span>
                            </td>
                        </tr>
                    </a:repeater>
                    <!-- 头费用对象信息 -->
                    <a:repeater datamodel="/model/exp_req_header_obj">
                        <tr>
                            <td class="line_content_title">
                                <span><![CDATA[${@object_name}：]]></span>
                            </td>
                            <td class="line_content_value">
                                <span><![CDATA[${@object_value_name}]]></span>
                            </td>
                        </tr>
                    </a:repeater>
                </table>
            </div>
        </div>
        <div id="line_frame">
            <div class="line_title">
                <span><![CDATA[申请单行信息]]></span>
            </div>
            <div>
                <a:freeMarker><![CDATA[
	            	<#if model.getObject("/model/exp_req_line")??>
	            		<#if  model.getObject("/model/exp_req_line").getChilds()??>
		            		<#list model.getObject("/model/exp_req_line").getChilds() as line>
		            		<div>
		            			<!-- 行数据 -->
		            			<fieldset class="line_frame">
		            				<legend>
		            					<span class="line_title">行号：${line.getString("line_number")}</span>
		            				</legend>
		            				<table class="line_content">
			            				<tr>
			            					<td class="line_content_title">
			            						<span>地点类型：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("place_type_name")??>
			            							<span>${line.getString("place_type_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>地点：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("place_name")??>
			            							<span>${line.getString("place_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>描述：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("description")??>
			            							<span>${line.getString("description")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>日期从：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("date_from")??>
			            							<span>${line.getString("date_from")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>日期到：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("date_to")??>
			            							<span>${line.getString("date_to")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>期间名：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("period_name")??>
			            							<span>${line.getString("period_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>币种：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("currency_code")??>
			            							<span>${line.getString("currency_code")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>汇率：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("exchange_rate")??>
			            							<span>${line.getString("exchange_rate")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>费用类型：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("expense_type_name")??>
			            							<span>${line.getString("expense_type_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>费用项目：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("expense_item_name")??>
			            							<span>${line.getString("expense_item_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>预算项目：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("budget_item_name")??>
			            							<span>${line.getString("budget_item_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>单价：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("price")??>
			            							<span>${line.getString("price")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>数量：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("primary_quantity")??>
			            							<span>${line.getString("primary_quantity")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>原币金额：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("requisition_amount")??>
			            							<span>${line.getString("requisition_amount")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>本币金额：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("requisition_functional_amount")??>
			            							<span>${line.getString("requisition_functional_amount")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>公司：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("company_short_name")??>
			            							<span>${line.getString("company_short_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>经营单位：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("operation_unit_name")??>
			            							<span>${line.getString("operation_unit_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>部门：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("unit_name")??>
			            							<span>${line.getString("unit_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>岗位：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("position_name")??>
			            							<span>${line.getString("position_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>成本中心：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("responsibility_center_name")??>
			            							<span>${line.getString("responsibility_center_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>报销人：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("employee_name")??>
			            							<span>${line.getString("employee_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>收款方类型：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("payee_type_name")??>
			            							<span>${line.getString("payee_type_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<tr>
			            					<td class="line_content_title">
			            						<span>收款方：</span>
			            					</td>
			            					<td class="line_content_value">
			            						<#if line.getString("payee_name")??>
			            							<span>${line.getString("payee_name")}</span>
			            						</#if>
			            					</td>
			            				</tr>
			            				<!-- 行维度 -->
			            				<#if model.getObject("/model/exp_req_line_dim")??>
				            				<#if model.getObject("/model/exp_req_line_dim").getChilds()??>
				            					<#list model.getObject("/model/exp_req_line_dim").getChilds() as line_dim>
					            					<#if line_dim.getString("exp_requisition_line_id") ??>
				            							<#if line_dim.getString("exp_requisition_line_id") == line.getString("exp_requisition_line_id")>
						            						<tr>
										            			<td class="line_content_title">
										            				<#if line_dim.getString("dimension_name")??>
										            					<span>${line_dim.getString("dimension_name")}：</span>
										            				</#if>
										            			</td>
										            			<td class="line_content_value">
										            				<#if line_dim.getString("dimension_value_name")??>
										            					<span>${line_dim.getString("dimension_value_name")}</span>
										            				</#if>
										            			</td>
										            		</tr>
				            							</#if>
				            						</#if>
				            					</#list>
			            					</#if>
										</#if>
										<#if model.getObject("/model/exp_req_line_obj")??>
											<#if model.getObject("/model/exp_req_line_obj").getChilds()??>
				            					<#list model.getObject("/model/exp_req_line_obj").getChilds() as line_obj>
					            					<#if line_obj.getString("expense_requisition_line_id") ??>
				            							<#if line_obj.getString("expense_requisition_line_id") == line.getString("exp_requisition_line_id")>
				            								<tr>
								            					<td class="line_content_title">
								            						<#if line_obj.getString("object_name")??>
								            							<span>${line_obj.getString("object_name")}：</span>
								            						</#if>
								            					</td>
								            					<td class="line_content_value">
								            						<#if line_obj.getString("object_value_name")??>
								            							<span>${line_obj.getString("object_value_name")}</span>
								            						</#if>
								            					</td>
								            				</tr>
				            							</#if>
				            						</#if>
				            					</#list>
			            					</#if>
										</#if>
			            			</table>
		            			</fieldset>
		            		</div>
		            		</#list>
	            		</#if>
	            	</#if>
	            ]]></a:freeMarker>
            </div>
        </div>
    </a:view>
</a:screen>
