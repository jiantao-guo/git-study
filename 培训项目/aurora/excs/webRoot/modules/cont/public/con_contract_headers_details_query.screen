            ]]></a:hBox>
            <a:hBox labelWidth="80">
                <a:textArea name="note" bindTarget="con_contract_header_ds" width="860"/>
            </a:hBox>
        </a:form>
        <a:hBox style="margin-top:5px;margin-left:5px">
