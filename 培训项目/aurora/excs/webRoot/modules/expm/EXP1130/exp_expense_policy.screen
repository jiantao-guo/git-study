                    </a:field>
                    <a:field name="expense_address" autoComplete="true" autoCompleteField="place_desc" lovGridHeight="340" lovHeight="470" lovLabelWidth="120" lovService="expm.exp_expense_policy_place_lov" lovWidth="550" title="PROMPT.PLACE_QUERY">
