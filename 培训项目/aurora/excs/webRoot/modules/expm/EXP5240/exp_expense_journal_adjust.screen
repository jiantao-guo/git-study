                    Aurora.showWarningMessage('', '${l:PROMPT.PLEASE_SELECT}', null, 200, 100);
                    return;
                }
                var datas = [];
                for (var i = 0,
                    l = records.length;i < l;i++) {
                    var data = {
                        exp_report_header_id: records[i]['exp_report_header_id'],
                        _status: 'execute'
                    };
                    datas.push(data);
                }
                Aurora.request({
                    url: $('EXP5250_delete_exp_report_accounts_link').getUrl(),
                    para: datas,
                    success: function() {
                        doQuery();
                    },
                    scope: this
                });
            }
            
            
