            function amountValidator(record, name, value) {
                if (name == "entered_amount_dr" || name == "entered_amount_cr") {
                    var from_value = record.get('entered_amount_dr');
                    var to_value = record.get('entered_amount_cr');
                    if(!(to_value || from_value) || (to_value && from_value)){
                        return '请输入借方金额或者贷方金额!';
                    }
                }
                
                return true;
            }
            function updateLines(dataSet, record, name, value, oldValue) {
            
               if (name === 'entered_amount_dr') {
                    if (value === '' || value === null) {
                        //设置借方金额可以为空
                        record.getField('entered_amount_dr').setRequired(false);
                        record.getField('functional_amount_dr').setRequired(false);
            
                        record.getField('functional_amount_dr').setReadOnly(false);
                        record.set('functional_amount_dr', '');
                        record.getField('functional_amount_dr').setReadOnly(true);
                        //设置贷方金额不可以为空
                        record.getField('entered_amount_cr').setRequired(true);
                        record.getField('functional_amount_cr').setRequired(true);
                    } else {
                        record.getField('functional_amount_dr').setReadOnly(false);
                        record.set('functional_amount_dr', value);
                        record.getField('functional_amount_dr').setReadOnly(true);
                        //设置贷方金额可以为空
                        record.getField('entered_amount_cr').setRequired(false);
                        record.getField('functional_amount_cr').setRequired(false);
                    }
                } else if (name === 'entered_amount_cr') {
                    if (value === '' || value === null) {
                        //设置贷方金额可以为空
                        record.getField('entered_amount_cr').setRequired(false);
                        record.getField('functional_amount_cr').setRequired(false);
            
                        record.getField('functional_amount_cr').setReadOnly(false);
                        record.set('functional_amount_cr', '');
                        record.getField('functional_amount_cr').setReadOnly(true);
                        //设置借方金额不可以为空
                        record.getField('entered_amount_dr').setRequired(true);
                        record.getField('functional_amount_dr').setRequired(true);
                    } else {
                        record.getField('functional_amount_cr').setReadOnly(false);
                        record.set('functional_amount_cr', value);
                        record.getField('functional_amount_cr').setReadOnly(true);
                        //设置借方金额可以为空
                        record.getField('entered_amount_dr').setRequired(false);
                        record.getField('functional_amount_dr').setRequired(false);
                    }
                }
               
            }
            //批量打印
            function batchPrintFunction() {
                var records = $('audit_result_ds').getJsonData(true);
                var record = $('audit_result_ds').getSelected();
                var reportName = record[0].get('report_name');
                var href = window.location.href;
                href = href.substring(0, href.indexOf("/modules"));
                if (records.length < 1) {
                    Aurora.showMessage('${l:PROMPT}', '${l:PROMPT.PLEASE_SELECT}');
                    return false;
                }
                for (var i = 0;i < records.length;i++) {
                    records[i]["web_url"] = href;
                    //生成凭证页面，单据本来就已经审核通过了
                    // if (record[i].get('requisition_status_value') != 'COMPLETELY_APPROVED') {
                    // Aurora.showInfoMessage('${l:PROMPT}', '请打印审批通过的单据!');
                    // return;
                    // }
                    var reportNameLine = record[i].get('report_name');
                    if (reportName != reportNameLine) {
                        Aurora.showMessage('${l:PROMPT}', '请选择同类型的单据打印!');
                        return;
                    }
                }
                new Aurora.request({
                    url: $('cux_exp_report_paste_temp_clear_link').getUrl(),
                    success: function(res) {
                        new Aurora.request({
                            url: $('cux_exp_report_paste_temp_insert_link').getUrl(),
                            para: records,
                            success: function(res) {
                                window.open('${/request/@context_path}/reports?__report=' + reportName + '&session_id=' + '${/session/@session_id}' + '&__format=pdf');
                            }
                        });
                    }
                });
            }
            
            function eam2000_notify_emp() {
                var record = $('audit_result_ds').getSelected();
                if (record.length < 1 || record.length > 1) {
                    Aurora.showMessage('${l:PROMPT}', '${l:CHOOSE_ONE_RECORD}');
                    return false;
                }
                var exp_report_header_id = record[0].get('exp_report_header_id');
                new Aurora.Window({
                    id: 'eam2000_notify_emp_screen',
                    url: $('eam2000_notify_emp_link').getUrl() + '?exp_report_header_id=' + exp_report_header_id,
                    title: '选择员工',
                    width: 680,
                    height: 420
                });
            
            }
