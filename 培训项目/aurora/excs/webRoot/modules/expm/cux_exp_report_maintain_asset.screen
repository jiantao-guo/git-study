                    if (description0 != linerecords[m].get('description')) {
                        Aurora.showErrorMessage('${l:PROMPT.ERROR}', '${l:EXP_REPORT_HEAD_DESCRIPTION}');
                        return false;
                    }
                }
                return true;
            }
            
            function setfunctionamount(value, record, name) {
