                if (vat_special_invoice_include == "Y") {
                    //报销单行新增字段隐藏
                    $('grid').showColumn('invoice_type_desc');
                    $('grid').showColumn('invoice_number');
                    $('grid').showColumn('invoice_status_desc');
                    $('grid').showColumn('invoice_code');
                    $('grid').showColumn('invoice_date');
