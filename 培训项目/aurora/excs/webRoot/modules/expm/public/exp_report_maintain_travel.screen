                    } else if (payee_category == 'EMPLOYEE') { /* f1.setLovService('csh.csh_pay_req_emp_lov'); */
                        // f1.setLovService('expm.exp_req_employee_lov');
                        f1.setLovService('csh.csh_pay_req_emp_lov_ext1');
