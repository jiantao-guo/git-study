    $Revision: 1.1 $
    $Purpose: "会计期间控制" 模块中，用于关闭一个期间的bm
-->
<model xmlns="http://www.aurora-framework.org/schema/bm">
    <operations>
        <operation name="execute">
            <update-sql><![CDATA[
                begin
                  gld_period_pkg.close_period(${/session/@company_id},
                                              ${@period_set_code},
                                              ${@period_name},
                                              ${/session/@user_id});
                end;
            ]]></update-sql>
        </operation>
    </operations>
</model>
