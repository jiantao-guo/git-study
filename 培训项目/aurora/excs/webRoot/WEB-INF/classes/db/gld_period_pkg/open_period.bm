<model xmlns="http://www.aurora-framework.org/schema/bm">
    <operations>
        <operation name="execute">
            <update-sql><![CDATA[
                begin
                  gld_period_pkg.open_period(${/session/@company_id},
                                             ${/parameter/@period_set_code},
                                             ${/parameter/@period_name},
                                             ${/session/@user_id});
                end;
            ]]></update-sql>
        </operation>
    </operations>
</model>
