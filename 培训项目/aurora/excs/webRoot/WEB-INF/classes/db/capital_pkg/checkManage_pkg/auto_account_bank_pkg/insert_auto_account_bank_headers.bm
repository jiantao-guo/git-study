<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
            	begin
				  check_account_pkg.revocation_check(p_hfm_ids  => ${@hfm_ids},
                         							 p_bank_ids => ${@bank_ids},
                         							 p_user_id  => ${/session/@user_id});
				end;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[	   
            begin
            	check_account_pkg.update_account_check_status(${@line_id},
            	                                              ${@table_name},
                                                              ${@check_status});
			end;
		]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="delete">
            <bm:update-sql><![CDATA[	   
            begin
            	check_account_pkg.delete_auto_account_bank(${@flag1},
            	                                           ${@flag2},
            	                                           ${@group_id},
            	                                           ${@account_type});
			end;
		]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[	   
            begin
            	check_account_pkg.manual_check(p_hfm_ids  => ${@hfm_ids},
                         p_bank_ids  => ${@bank_ids},
                         p_user_id   => ${/session/@user_id});
			end;
		]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
