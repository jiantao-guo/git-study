<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[begin
			pur_purchase_requisition_pkg.ins_pur_requisition_dists
			(
				${@pur_requisition_dists_id},
				 ${@line_id}    ,
				 ${@line_number},
				 ${@description}                ,
				 ${@period_num}                ,
				 ${@price}                      ,
				 ${@primary_quantity}           ,
				 ${@requisition_amount}         ,
				 ${@company_id}                 ,
				 ${@UNIT_ID}                    ,
				 ${@POSITION_ID}                ,
				 ${@responsibility_center_id}   ,
				 ${@employee_id}                ,
				 ${@contract_head_id},   
				 ${@E1}              ,
				 ${@E2}              ,
				 ${@E3}              ,
				 ${@E4}              ,
				 ${@E5}              ,
				 ${@E6}              ,
				 ${@E7}              ,
				 ${@E8}              ,
				 ${@E9}              ,
				 ${@E10}   ,
				 ${/session/@user_id} 
			   );
			  	
		end;
		]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="pur_requisition_dists_id" dataType="java.lang.String" input="false" inputPath="@pur_requisition_dists_id" output="true" outputPath="@pur_requisition_dists_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
