<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    exp_report_pkg.insert_exp_report_dists (
                    p_exchange_rate => ${@exchange_rate}, 
                    p_created_by => ${/session/@user_id}, 
                    p_report_func_amount =>${@requisition_functional_amount}, 
                    p_responsibility_center_id =>${@responsibility_center_id}, 
                    p_report_amount =>${@requisition_amount}, 
                    p_exp_report_line_id =>${@line_id}, 
                    p_currency_code =>${@currency_code}, 
                    p_dimension2_id =>${@e2}, 
                    p_dimension4_id =>${@e4}, 
                    p_dimension3_id =>${@e3}, 
                    p_dimension1_id =>${@e1}, 
                    p_dimension10_id =>${@e10}, 
                    p_close_flag =>'n', 
                    p_dimension9_id =>${@e9}, 
                    p_dimension6_id =>${@e6}, 
                    p_dimension5_id =>${@e5}, 
                    p_dimension8_id =>${@e8}, 
                    p_dimension7_id =>${@e7}, 
                    p_expense_type_id =>${@expense_type_id}, 
                    p_expense_item_id =>${@expense_item_id}, 
                    p_price =>${@price}, 
                    p_primary_quantity =>${@primary_quantity}, 
                    p_period_name =>${@period_num}, 
                    p_description =>${@description}, 
                    p_exchange_rate_type =>${@exchange_rate_type}, 
                    p_exchange_rate_quotation =>${@exchange_rate_quotation}, 
                    p_payee_category =>${@payee_category}, 
                    p_employee_id =>${@employee_id}, 
                    p_payee_id =>${@payee_id}, 
                    p_company_id =>${@company_id}, 
                    p_unit_id =>${@unit_id}, 
                    p_exp_report_dists_id =>${@exp_requisition_dists_id}, 
		    		p_position_id =>${@position_id}
					p_secondary_uom=>${@secondary_uom},                        
					p_partner_id=>${@partner_id},
					p_secondary_quantity=>${@secondary_quantity},
					p_primary_uom=>${@primary_uom},
					p_close_date=>${@close_date},
					p_attachment_num=>${@attachment_num},
                    p_exceed_budget_strategy=>${@exceed_budget_strategy},
                    p_sale_amount=>${@sale_amount},
                    p_tax_amount=>${@tax_amount},
                    p_tax_rate=>${@tax_rate},
                    p_input_tax_structure_detail=>${@input_tax_structure_detail},
                    p_deduction_rules=>${@deduction_rules},
                    p_adjusted_full_deductions=>${@adjusted_full_deductions},
                    p_adjusted_partial_deductions=>${@adjusted_partial_deductions},
                    p_adjustable_tax_deductible=>${@adjustable_tax_deductible},
                    p_invoice_amount=>${@invoice_amount},
                    p_invoice_tax_amount=>${@invoice_tax_amount},
                    p_usage_type=>${@usage_type});
            ]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="exp_requisition_dists_id" dataType="java.lang.Long" output="true" outputPath="@exp_requisition_dists_id"/>
            </bm:parameters>
        </bm:operation>
    </bm:operations>
</bm:model>
