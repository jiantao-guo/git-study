<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            		select t.batch_id,
						   t.line_num,
						   t.workflow_code,
						   t.company_code,
						   t.transaction_category,
						   t.type_code,
						   t.node_name,
						   t.amount
					  from wfl_node_amount_imp t
					 where t.batch_id = ${/session/@session_id}
					   and t.created_by = ${/session/@user_id}
					 order by t.line_num
                 ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
