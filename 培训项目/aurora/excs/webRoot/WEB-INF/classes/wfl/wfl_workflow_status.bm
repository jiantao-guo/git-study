<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.aurora-framework.org/schema/bm" xmlns:ns2="aurora.database.local.oracle">
    <operations>
        <operation name="query">
            <query-sql>
                select to_number(code_value) as value,
                       code_value_name as name
                  from sys_code_values_v
                 where code = 'WFL_WORKFLOW_STATUS' 
            </query-sql>
        </operation>
    </operations>
</model>
