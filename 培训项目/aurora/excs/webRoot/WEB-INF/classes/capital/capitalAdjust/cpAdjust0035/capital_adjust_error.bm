<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    *
                FROM
                    (SELECT
                        t.CA_CAPITAL_ADJUST_LINE_ID,
                        t.CA_CAPITAL_ADJUST_INFO_ID,
                        t.CAP_USE_CN,
                        t.TRANS_OPPONET_CN,
                        t.OUT_IN_TYPE_CN,
                        t.GROUP_ACCOUNT_CN,
                        t.OUT_ACCOUNT_NAME,
                        t.OUT_ACCOUNT,
                        t.CURRENCY,
                        t.ADJUST_AMOUNT,
                        t.IN_ACCOUNT_NAME,
                        t.IN_ACCOUNT,
                        t.DESCRIPTION,
                        t.BATCH_ID,
                        t.IN_PROJECT,
                        t.OUT_PROJECT, l.error_message
                    FROM
                        ca_capital_adjust_tmp t,
                        ca_capital_adjust_err l
                    WHERE
                        t.ca_capital_adjust_line_id=l.ca_capital_adjust_line_id
                        and t.ca_capital_adjust_info_id = ${@ca_capital_adjust_info_id}
                    ) o #WHERE_CLAUSE#
                ORDER BY
                    ca_capital_adjust_line_id
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
