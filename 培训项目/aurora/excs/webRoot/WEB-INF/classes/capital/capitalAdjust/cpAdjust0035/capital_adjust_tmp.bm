<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    *
                FROM
                    (SELECT
                        CA_CAPITAL_ADJUST_LINE_ID,
                        CA_CAPITAL_ADJUST_INFO_ID,
                        CAP_USE_CN,
                        TRANS_OPPONET_CN,
                        OUT_IN_TYPE_CN,
                        GROUP_ACCOUNT_CN,
                        OUT_ACCOUNT_NAME,
                        OUT_ACCOUNT,
                        CURRENCY,
                        ADJUST_AMOUNT,
                        IN_ACCOUNT_NAME,
                        IN_ACCOUNT,
                        DESCRIPTION,
                        BATCH_ID,
                        IN_PROJECT,
                        OUT_PROJECT,
                        plan_date_time
                    FROM
                        ca_capital_adjust_tmp t
                    ) o #WHERE_CLAUSE#
                ORDER BY
                    ca_capital_adjust_line_id
            ]]></bm:query-sql>
        </bm:operation>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
            begin
            cp_capital_adjust_import_pkg.insert_interface(p_header_id =>${@header_id},
            										p_ca_capital_adjust_info_id =>${@ca_capital_adjust_info_id},
                                                           p_batch_id  =>${@batch_id},
                                                           p_user_id   =>${/session/@user_id});
            end;]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                   cp_capital_adjust_import_pkg.check_data(p_batch_id    =>${@batch_id},
							                       p_ca_capital_adjust_info_id =>${@ca_capital_adjust_info_id},
							                       p_user_id         =>${/session/@user_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
                BEGIN
                    cp_capital_adjust_import_pkg.submit_data(p_ca_capital_adjust_info_id =>${@ca_capital_adjust_info_id},
                                                      p_batch_id  =>${@batch_id},
                                                      p_user_id   =>${/session/@user_id});
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="ca_capital_adjust_info_id" queryExpression="ca_capital_adjust_info_id = ${@ca_capital_adjust_info_id}"/>
    </bm:query-fields>
</bm:model>
