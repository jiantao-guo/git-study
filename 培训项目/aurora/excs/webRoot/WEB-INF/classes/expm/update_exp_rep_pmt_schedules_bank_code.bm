<!--20130401  修改 计划付款行银行账号可维护  -->
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
                begin
                    EXP_REPORT_PKG.update_exp_rep_pmt_sch_bank
                    (
                        ${@exp_report_header_id},
                        ${@payment_schedule_line_id},
                        ${@account_number},
                        ${@account_name},
                        ${@bank_code},
                        ${@bank_name},
                        ${@bank_location_code},--分行代码
                        ${@bank_location_name},--分行名称
                        ${@province_code},
                        ${@province_name},
                        ${@city_code},
                        ${@city_name},
                        ${/session/@user_id}    
                    );
                end;]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                begin
                    EXP_REPORT_PKG.update_exp_rep_pmt_sch_bank
                    (
                        ${@exp_report_header_id},
                        ${@payment_schedule_line_id},
                        ${@account_number},
                        ${@account_name},
                        ${@bank_code},
                        ${@bank_name},
                        ${@bank_location_code},
                        ${@bank_location_name},
                        ${@province_code},
                        ${@province_name},
                        ${@city_code},
                        ${@city_name},
                        ${/session/@user_id}    
                    );
                end;]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="delete">
            <bm:update-sql><![CDATA[
                begin
                    EXP_REPORT_PKG.DELETE_EXP_REP_PMT_SCHEDULES
                    (
                        ${@payment_schedule_line_id},
                        ${@exp_report_header_id},
                        ${/session/@user_id}
                    );
                end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
