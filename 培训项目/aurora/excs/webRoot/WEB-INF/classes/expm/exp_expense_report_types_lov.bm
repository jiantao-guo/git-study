<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:f="aurora.database.features" xmlns:bm="http://www.aurora-framework.org/schema/bm" alias="eertvl" baseTable="EXP_EXPENSE_REPORT_TYPES_VL" defaultOrderBy="expense_report_type_id">
