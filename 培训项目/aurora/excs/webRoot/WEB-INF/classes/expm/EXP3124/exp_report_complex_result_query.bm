<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" defaultOrderBy="exp_report_number">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            select *
  from (select distinct h.exp_report_header_id,
               h.exp_report_number,
               (select fd.description_text
                  from bgt_budget_items t, fnd_descriptions fd
                 where t.budget_item_id = l.budget_item_id
                   and t.description_id = fd.description_id
                   and fd.language = ${/session/@lang}) budget_item_id,
               (select fd.description_text
                  from gld_accounts ga, fnd_descriptions fd
                 where ga.account_id = a.account_id
                   and ga.description_id = fd.description_id
                   and fd.language = ${/session/@lang}) audit_account_id,
               to_char(a.journal_date, 'yyyy-mm-dd') journal_date,
               r.exp_requisition_header_id,
               (select greatest((select sum(erl.requisition_amount) -
                       sum(err.req_release_amount)
                  from exp_requisition_release err, exp_requisition_lines erl
                 where err.document_line_id = l.exp_report_line_id
                   and err.exp_requisition_line_id =
                       erl.exp_requisition_line_id),0) from dual) req_not_report_money,
               (select erh.exp_requisition_number
                  from exp_requisition_headers erh
                 where erh.exp_requisition_header_id =
                       r.exp_requisition_header_id) ref_requisition_num,
               (select y.description
                  from exp_expense_report_types_vl y
                 where y.expense_report_type_id = h.exp_report_type_id) exp_report_type,
               (select e.name
                  from exp_employees e
                 where e.employee_id = h.employee_id) report_employee,
               (select t.description
                  from exp_expense_types_vl t
                 where t.expense_type_id = d.expense_type_id) expense_type,
               (select i.description
                  from exp_expense_items_vl i
                 where i.expense_item_id = d.expense_item_id) expense_item,
               a.period_name,
               (select c.COMPANY_SHORT_NAME
                  from fnd_companies_vl c
                 where c.company_id = d.company_id) company,
               (select r.responsibility_center_name
                  from fnd_responsibility_centers_vl r
                 where r.responsibility_center_id =
                       d.responsibility_center_id) res_center,
               (select u.DESCRIPTION
                  from exp_org_unit_vl u
                 where u.UNIT_ID = d.unit_id) unit,
               (select e.name
                  from exp_employees e
                 where e.employee_id = d.employee_id) bear_employee,
               d.price,
               d.primary_quantity,
               d.report_amount,
               d.exchange_rate,
               d.report_functional_amount,
               d.currency_code,
               d.report_status report_status_value,
               (select b.code_value_name
                  from sys_codes_vl a, sys_code_values_vl b
                 where a.code_id = b.code_id
                   and a.code = 'EXP_EXPENSE_REPORT_STATUS'
                   and b.code_value = d.report_status) report_status,
               d.description,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension1_id) d1,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension2_id) d2,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension3_id) d3,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension4_id) d4,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension5_id) d5,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension6_id) d6,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension7_id) d7,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension8_id) d8,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension9_id) d9,
               (select vt.description
                  from fnd_dimension_values_vl vt
                 where vt.dimension_value_id = d.dimension10_id) d10,
		 (select sum(d.report_amount)  from exp_report_dists_session_tmp t,
               exp_report_dists             d,
               exp_report_headers           h,
               exp_report_lines             l,
               (select *
                          from exp_report_accounts era
                         where era.usage_code = 'EMPLOYEE_EXPENSE') a,
               exp_requisition_release      r
         where d.exp_report_line_id = l.exp_report_line_id
           and l.exp_report_header_id = h.exp_report_header_id
           and t.session_id = ${/session/@session_id}
           and t.exp_report_dists_id = d.exp_report_dists_id
           and a.exp_report_dists_id(+) = t.exp_report_dists_id
           and r.document_line_id(+) = l.exp_report_line_id      ) sum_report_amount,
		 (select greatest((select sum(erl.requisition_amount) -
                       sum(err.req_release_amount)
                  from exp_requisition_release err, exp_requisition_lines erl
                 where err.document_line_id = l.exp_report_line_id
                   and err.exp_requisition_line_id =
                       erl.exp_requisition_line_id),0) from dual) sum_req_not_report_money
          from exp_report_dists_session_tmp t,
               exp_report_dists             d,
               exp_report_headers           h,
               exp_report_lines             l,
               (select *
                          from exp_report_accounts era
                         where era.usage_code = 'EMPLOYEE_EXPENSE') a,
               exp_requisition_release      r
         where d.exp_report_line_id = l.exp_report_line_id
           and l.exp_report_header_id = h.exp_report_header_id
           and t.session_id = ${/session/@session_id}
           and t.exp_report_dists_id = d.exp_report_dists_id
           and a.exp_report_dists_id(+) = t.exp_report_dists_id
           and r.document_line_id(+) = l.exp_report_line_id        
        ),
       sys_user s
       where s.user_id=${/session/@user_id}


		#ORDER_BY_CLAUSE#
		 	]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
