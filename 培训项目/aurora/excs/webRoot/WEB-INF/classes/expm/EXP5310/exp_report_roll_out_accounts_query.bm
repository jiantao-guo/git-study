		       where rownum = 1 and grs.rule_id = gra.rule_id  and gra.enabled_flag = 'Y' and grs.enabled_flag = 'Y' and gs.enabled_flag = 'Y'
		       and grs.segment_id = gs.segment_id 
		       and gra.account_id = t.account_id and exists (select 1 from gld_interface_rules r where r.set_of_books_id = fc.set_of_books_id and r.rule_id = grs.rule_id) 
		       and gs.segment_field = 'SEGMENT9') as segment9_id,
		              (select gs.segment_id
		  from gld_rule_accounts   gra,
		       gld_rule_segments   grs,
		       gld_segments        gs 
		       where rownum = 1 and grs.rule_id = gra.rule_id  and gra.enabled_flag = 'Y' and grs.enabled_flag = 'Y' and gs.enabled_flag = 'Y'
