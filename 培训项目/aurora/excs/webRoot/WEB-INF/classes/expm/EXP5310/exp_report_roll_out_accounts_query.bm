<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            SELECT t2.* FROM (
   SELECT T.EXP_REPORT_DISTS_ID,
       T.EXP_REPORT_JE_LINE_ID,
       T.payment_schedule_line_id,
