<?xml version="1.0" encoding="UTF-8"?><bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select d.code_value value, d.code_value_name name
         from sys_codes t, sys_code_values_vl d
        where t.code = 'BANK_TYPE'
          and t.code_id = d.code_id]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
