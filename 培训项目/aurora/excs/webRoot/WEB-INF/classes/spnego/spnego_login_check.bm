<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[            
               	 select t.service_name from sys_service t where t.is_login_required=0 and t.service_name=${/request/@service_name}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
