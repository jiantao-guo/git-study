<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select count(1) as token_count from mbs_device_user_binding b where b.device_token = ${@token}]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
