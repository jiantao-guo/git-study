<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:e="aurora.service.exception" xmlns:o="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="GLD_SOB_WO_TYPE_ASSIGN_COMP">
    <bm:fields>
        <bm:field name="work_order_type_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="WORK_ORDER_TYPE_ID"/>
        <bm:field name="company_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="COMPANY_ID"/>
        <bm:field name="enabled_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ENABLED_FLAG"/>
        <bm:field name="company_code" forInsert="false" forUpdate="false"/>
        <bm:field name="company_desc" forInsert="false" forUpdate="false"/>
    </bm:fields>
    <bm:features>
        <f:standard-who/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="work_order_type_id"/>
        <bm:pk-field name="company_id"/>
    </bm:primary-key>
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    *
                FROM
                    (SELECT
                        t1.work_order_type_id,
                        t1.company_id,
                        t1.enabled_flag,
                        (SELECT
                            f.company_code
                        FROM
                            fnd_companies_vl f
                        WHERE
                            f.company_id =t1.company_id
                        )company_code,
                        (SELECT
                            f.company_short_name
                        FROM
                            fnd_companies_vl f
                        WHERE
                            f.company_id =t1.company_id
                        )company_desc
                    FROM
                        GLD_SOB_WO_TYPE_ASSIGN_COMP t1
                    )v #WHERE_CLAUSE#
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:query-fields>
        <bm:query-field name="work_order_type_id" queryExpression="work_order_type_id=${@work_order_type_id}"/>
    </bm:query-fields>
    <e:exception-descriptor-config>
        <e:exception-descriptor exception="java.sql.SQLException" handleClass="aurora.database.SQLExceptionDescriptor">
            <e:error-message code="1" message="GLD1040_SOB_WORK_ORDER.ASSIGNED_REPEAT"/>
        </e:exception-descriptor>
    </e:exception-descriptor-config>
</bm:model>
