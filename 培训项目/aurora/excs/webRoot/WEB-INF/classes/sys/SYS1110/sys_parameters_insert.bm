<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.aurora-framework.org/schema/bm" xmlns:ns2="aurora.database.local.oracle">
    <operations>
        <operation name="insert">
            <update-sql>
				begin
				  sys_parameter_pkg.insert_parameter(${@module_id},
				                                     ${@parameter_code},
				                                     ${@validation_type},
				                                     ${@description},
				                                     ${@user_changeable_flag},
				                                     ${@user_visible_flag},
				                                     ${@system_enabled_flag},
				                                     ${@role_enabled_flag},
				                                     ${@user_enabled_flag},
				                                     ${@sql_validation},
				                                     to_date(${@start_date_active},'yyyy-mm-dd'),
				                                     to_date(${@end_date_active},'yyyy-mm-dd'),
				                                     ${@parameter_name},
				                                     ${@encrypt_value_flag},
				                                     null,
				                                     ${/session/@user_id},
				                                     ${@company_enabled_flag});
				end;
            </update-sql>
        </operation>
        <operation name="update">
            <update-sql>
                begin
				  sys_parameter_pkg.update_parameter(${@module_id},
				                                     ${@parameter_id},
				                                     ${@validation_type},
				                                     ${@description},
				                                     ${@user_changeable_flag},
				                                     ${@user_visible_flag},
				                                     ${@system_enabled_flag},
				                                     ${@role_enabled_flag},
				                                     ${@user_enabled_flag},
				                                     ${@sql_validation},
				                                     to_date(${@start_date_active},'yyyy-mm-dd'),
				                                     to_date(${@end_date_active},'yyyy-mm-dd'),
				                                     ${@parameter_name},
				                                     ${@encrypt_value_flag},
				                                     null,
				                                     ${/session/@user_id},
				                                     ${@company_enabled_flag});
				end;

            </update-sql>
        </operation>
    </operations>
</model>