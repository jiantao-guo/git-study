<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            	select cu.precision from gld_currency_vl cu where cu.currency_code = ${@currency_code}
             ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="precision" databaseType="varchar2" datatype="java.lang.String" forDisplay="true" physicalName="address" prompt="地址"/>
    </bm:fields>
</bm:model>
