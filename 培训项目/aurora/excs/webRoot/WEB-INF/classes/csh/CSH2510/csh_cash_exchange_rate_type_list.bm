<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select etv.type_code type_code,
                   etv.type_name type_name
        from gld_exchangerate_types_vl etv
       where etv.enabled_flag = 'Y'
       order by etv.type_code]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
