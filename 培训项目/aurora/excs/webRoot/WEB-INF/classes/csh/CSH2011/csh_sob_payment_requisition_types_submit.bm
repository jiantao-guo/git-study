<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
            begin
				csh_pay_req_types_pkg.insert_sob_csh_pay_req_types(
 												p_set_of_books_id => ${/parameter/@set_of_books_id},
												p_type_code  => ${@type_code},
												p_company_id => ${/session/@company_id},
												p_description =>${@description},
												p_currency_code=>${@currency_code},
                      							p_approve_flag =>${@auto_approve_flag},
                      							p_enabled_flag =>${@enabled_flag},
                      							p_user_id    =>${/session/@user_id},
                      							p_report_name  =>${@report_name},
                      							p_payment_method =>${@payment_method},
                      							p_type_id    =>${@type_id},
                      							p_mobile_approve => ${@mobile_approve},                      							
                      							p_mobile_fill => ${@mobile_fill},
                      							p_app_documents_icon => ${@app_documents_icon}                         
		                          ); 
          	end;
]]></bm:update-sql>
            <bm:parameters>
                <bm:parameter name="type_id" dataType="java.lang.Long" input="false" inputPath="@type_id" output="true" outputPath="@type_id"/>
            </bm:parameters>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
                BEGIN
                    csh_pay_req_types_pkg.update_sob_csh_pay_req_types(
                    ${@type_id}, 
                    ${@type_code}, 
                    ${/parameter/@set_of_books_id}, 
                    ${@description}, 
                    ${@currency_code}, 
                    ${@auto_approve_flag}, 
                    ${@enabled_flag}, 
                    ${/session/@user_id}, 
                    ${@report_name}, 
                    ${@payment_method}, 
                    ${@mobile_approve}, 
                    ${@mobile_fill}, 
                    ${@app_documents_icon});
                END;
            ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
