<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="insert">
            <bm:update-sql><![CDATA[
	    begin
			csh_transaction_pkg.insert_csh_transaction_header
			(
				${/session/@company_id},
				${@payment_header_id},
				'PAYMENT',
				to_date(${@transaction_date},'yyyy-mm-dd'),
				${@period_name},
				${@payment_methods},
				${@transaction_category},
				'N',
				NULL,
				NULL,
				'N',
				'N',
				NULL,
				NULL,
				'N',
				null,
				'Y',
				${@contract_header_id},
				${/session/@user_id}
			);	
		end;]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
