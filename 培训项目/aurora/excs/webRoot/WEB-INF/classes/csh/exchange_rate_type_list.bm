<?xml version="1.0" encoding="UTF-8"?><bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[			SELECT ETV.TYPE_CODE, ETV.TYPE_NAME
			  FROM GLD_EXCHANGERATE_TYPES_VL ETV
			 WHERE ETV.ENABLED_FLAG = 'Y'
			 ORDER BY ETV.TYPE_CODE]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="type_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="TYPE_CODE" required="true"/>
        <bm:field name="type_name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="TYPE_NAME"/>
    </bm:fields>
</bm:model>
