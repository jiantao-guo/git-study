<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select * from (select sob.SET_OF_BOOKS_ID,
				   sob.SET_OF_BOOKS_CODE || '-' || sob.SET_OF_BOOKS_NAME sob_name
			  from gld_set_of_books_vl sob
			 where sob.ENABLED_FLAG = 'Y'
			 order by sob.SET_OF_BOOKS_CODE )#WHERE_CLAUSE#]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="set_of_books_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="SET_OF_BOOKS_ID"/>
        <bm:field name="sob_name" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="SOB_NAME"/>
    </bm:fields>
    <bm:query-fields>
        <bm:query-field name="set_of_books_id" dataType="java.lang.Long" queryExpression="set_of_books_id = ${@set_of_books_id}"/>
    </bm:query-fields>
</bm:model>
