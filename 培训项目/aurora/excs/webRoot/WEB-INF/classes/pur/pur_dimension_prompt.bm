<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 1) prompt1,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 2) prompt2,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 3) prompt3,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 4) prompt4,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 5) prompt5,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 6) prompt6,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 7) prompt7,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 8) prompt8,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 9) prompt9,
       (select fv.description
          from fnd_dimensions_vl fv
         where fv.dimension_sequence = 10) prompt10
  from fnd_dimensions_vl
]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
