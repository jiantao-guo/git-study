<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select h.bgt_org_id,       h.bgt_solution_header_id,       h.bgt_solution_code,       h.description     from bgt_solution_headers_vl h    where h.bgt_solution_header_id = ${/parameter/@bgt_solution_header_id}]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="bgt_org_id" databaseType="NUMBER" datatype="java.lang.Double" physicalName="BGT_ORG_ID"/>
        <bm:field name="bgt_solution_header_id" databaseType="NUMBER" datatype="java.lang.Double" physicalName="BGT_SOLUTION_HEADER_ID"/>
        <bm:field name="bgt_solution_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BGT_SOLUTION_CODE"/>
        <bm:field name="description" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="DESCRIPTION"/>
    </bm:fields>
</bm:model>
