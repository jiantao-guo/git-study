<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                 SELECT a.asset_book_id,
				       a.asset_book_code,
				       a.description asset_book_desc
				  FROM eam_asset_books_vl a,fnd_companies c
			          where a.SET_OF_BOOK_ID = c.set_of_books_id
			          and c.company_id = ${/session/@company_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
