<bm:model xmlns:e="aurora.service.exception" xmlns:ns2="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t" baseTable="EAM_REQUISITION_TYPES" defaultOrderBy="t.ast_requisition_type_code asc">
    <bm:fields>
        <bm:field name="ast_requisition_type_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="AST_REQUISITION_TYPE_ID" prompt="EAM_REQUISITION_TYPES.REQUISITION_TYPE_ID"/>
        <bm:field name="asset_book_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="ASSET_BOOK_ID" prompt="EAM_REQUISITION_TYPES.ASSET_BOOK_ID"/>
        <bm:field name="asset_book_desc" databaseType="NUMBER" datatype="java.lang.String" expression="(select v.description from eam_asset_books_vl v where v.asset_book_id = t.asset_book_id)" forInsert="false" forUpdate="false" prompt="EAM_ASSET_BOOKS.DESCRIPTION"/>
        <bm:field name="ast_requisition_type_code" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="AST_REQUISITION_TYPE_CODE" prompt="EAM_REQUISITION_TYPES.REQUISITION_TYPE_CODE"/>
        <bm:field name="description_id" databaseType="NUMBER" datatype="java.lang.Long" multiLanguage="true" multiLanguageDescField="description" physicalName="DESCRIPTION_ID" prompt="EAM_REQUISITION_TYPES.DESCRIPTION_ID"/>
        <bm:field name="page_service" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="PAGE_SERVICE" prompt="EAM_REQUISITION_TYPES.PAGE_SERVICE"/>
        <bm:field name="base_trx_category" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BASE_TRX_CATEGORY" prompt="EAM_REQUISITION_TYPES.BASE_TRX_CATEGORY"/>
        <bm:field name="base_trx_category_desc" expression="(SELECT v.code_value_name   FROM sys_service_lookup_v v  WHERE v.enabled_flag = &apos;Y&apos;    AND V.code_value = t.base_trx_category    AND code = &apos;EAM_BASE_TRX_CATEGORY&apos;    and language = ${/session/@lang})" forInsert="false" forUpdate="false" prompt="EAM_REQUISITION_TYPES.BASE_TRX_CATEGORY"/>
        <bm:field name="default_trx_type_id" databaseType="NUMBER" datatype="java.lang.Long" physicalName="DEFAULT_TRX_TYPE_ID" prompt="EAM_REQUISITION_TYPES.DEFAULT_TRX_TYPE_ID"/>
        <bm:field name="default_trx_type_desc" expression="(select f.description_text from eam_transaction_types t,fnd_descriptions f         where t.description_id = f.description_id and t.transaction_type_id = t.default_trx_type_id and f.language = ${/session/@lang})" forInsert="false" forUpdate="false" prompt="EAM_REQUISITION_TYPES.DEFAULT_TRX_TYPE_DESC"/>
        <bm:field name="auto_approve_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="AUTO_APPROVE_FLAG" prompt="EAM_REQUISITION_TYPES.AUTO_APPROVE_FLAG"/>
        <bm:field name="auto_audit_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="AUTO_AUDIT_FLAG" prompt="EAM_REQUISITION_TYPES.AUTO_AUDIT_FLAG"/>
        <bm:field name="account_manual_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ACCOUNT_MANUAL_FLAG" prompt="EAM_REQUISITION_TYPES.ACCOUNT_MANUAL_FLAG"/>
        <bm:field name="auto_confirm_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="AUTO_CONFIRM_FLAG" prompt="EAM_REQUISITION_TYPES.AUTO_CONFIRM_FLAG"/>
        <bm:field name="bgt_reverse_type" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BGT_REVERSE_TYPE" prompt="EAM_REQUISITION_TYPES.BGT_REVERSE_TYPE"/>
        <bm:field name="bgt_reverse_type_desc" expression="(select v.code_value_name from sys_code_values_v v where v.code = &apos;EAM_BGT_REVERSE_TYPE&apos; and v.code_value = t.bgt_reverse_type)" forInsert="false" forUpdate="false" prompt="EAM_REQUISITION_TYPES.BGT_REVERSE_TYPE"/>
        <bm:field name="bgt_ctrl_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="BGT_CTRL_FLAG" prompt="EAM_REQUISITION_TYPES.BGT_CTRL_FLAG"/>
        <bm:field name="ref_required_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="REF_REQUIRED_FLAG" prompt="EAM_REQUISITION_TYPES.REF_REQUIRED_FLAG"/>
        <bm:field name="amount_control" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="AMOUNT_CONTROL" prompt="EAM_REQUISITION_TYPES.AMOUNT_CONTROL"/>
        <bm:field name="enabled_flag" databaseType="VARCHAR2" datatype="java.lang.String" physicalName="ENABLED_FLAG" prompt="EAM_REQUISITION_TYPES.ENABLED_FLAG"/>
        <bm:field name="description" databaseType="VARCHAR" datatype="java.lang.String" forInsert="false" forUpdate="false" prompt="EAM_REQUISITION_TYPES.DESCRIPTION"/>
        <!-- <bm:field name="company_id" databaseType="NUMBER" datatype="java.lang.Long" insertExpression="${/session/@company_id}" updateExpression="${/session/@company_id}"/> -->
    </bm:fields>
    <bm:features>
        <ns2:sequence-pk/>
        <f:standard-who/>
        <f:multi-language-storage/>
    </bm:features>
    <bm:primary-key>
        <bm:pk-field name="ast_requisition_type_id"/>
    </bm:primary-key>
    <bm:query-fields>
        <bm:query-field field="ast_requisition_type_id" queryOperator="="/>
        <bm:query-field field="asset_book_desc" queryExpression="exists (select 1 from eam_asset_books_vl eabv where t.asset_book_id=eabv.asset_book_id and eabv.description like ${@asset_book_desc})"/>
        <bm:query-field field="ast_requisition_type_code" queryOperator="like"/>
        <bm:query-field field="description" queryExpression="exists (select 1 from fnd_descriptions fd where t.description_id=fd.description_id and fd.description_text like ${@description} and fd.language = ${/session/@lang})"/>
    </bm:query-fields>
    <e:exception-descriptor-config>
        <e:exception-descriptor exception="java.sql.SQLException" handleClass="aurora.database.SQLExceptionDescriptor">
            <e:error-message code="1" message="EAM_REQUISITION_TYPES.EAM_REQUISITION_TYPES_DUPLICATE"/>
        </e:exception-descriptor>
    </e:exception-descriptor-config>
    <!-- <bm:data-filters> -->
    <!-- <bm:data-filter enforceOperations="query" expression="t.asset_book_id = (SELECT A.ASSET_BOOK_ID FROM EAM_ASSET_BOOKS_VL A WHERE A.COMPANY_ID = ${/session/@company_id})"/> -->
    <!--  <bm:data-filter enforceOperations="query" expression="t.company_id=${/session/@company_id}"/> -->
    <!-- </bm:data-filters> -->
</bm:model>
