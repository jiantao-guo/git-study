<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    ll.*,
                    (SELECT ss.description FROM sys_user ss WHERE ss.user_id = ll.employee_id
                    ) employee_name
                FROM
                    WFL_ADD_APPROVAL_LIST ll
                WHERE
                    ll.approval_list_id = ${@approval_list_id}
                ORDER BY
                    ll.seq_number
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
