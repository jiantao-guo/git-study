<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    SUM(l.requisition_amount) req_amount_sum
                FROM
                    exp_requisition_lines l
                WHERE
                    l.exp_requisition_header_id = ${@exp_requisition_header_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
